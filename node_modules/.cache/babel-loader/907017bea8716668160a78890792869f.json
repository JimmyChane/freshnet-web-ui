t\t\treturn filters\r\n\t\t\t\t\t.filter((filter) => filter.count >= valueToPass)\r\n\t\t\t\t\t.sort((filter1, filter2) => filter1.count - filter2.count)\r\n\t\t\t\t\t.map((filter) => filter.item);\r\n\t\t\t},\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<template>\r\n\t<div class=\"Actionbar\">\r\n\t\t<ActionbarMenus\r\n\t\t\tclass=\"Actionbar-leftMenus\"\r\n\t\t\tv-if=\"$root.navigation.isDrawer()\"\r\n\t\t\t:menus=\"[\r\n\t\t\t\t{\r\n\t\t\t\t\tkey: 'hamburgerMenu',\r\n\t\t\t\t\ttitle: 'Hamburger Menu',\r\n\t\t\t\t\ticon: host.res('icon/hamburgerMenu-2A4858.svg'),\r\n\t\t\t\t\tclick: () => $emit('click-drawer-expand'),\r\n\t\t\t\t},\r\n\t\t\t]\"\r\n\t\t/>\r\n\r\n\t\t<SearchInput\r\n\t\t\tclass=\"Actionbar-search\"\r\n\t\t\tv-if=\"items.length\"\r\n\t\t\tplaceholder=\"Search customers & devices\"\r\n\t\t\t:list=\"results\"\r\n\t\t\t@callback-search=\"(str) => (results = searchResults(str))\"\r\n\t\t\tv-slot=\"{ collapse }\"\r\n\t\t>\r\n\t\t\t<router-link\r\n\t\t\t\tclass=\"Actionbar-search-link\"\r\n\t\t\t\tv-for=\"item in results\"\r\n\t\t\t\t:key=\"item.id\"\r\n\t\t\t\t:to=\"{ query: { customerId: item.id } }\"\r\n\t\t\t\t@click=\"\r\n\t\t\t\t\t() => {\r\n\t\t\t\t\t\tcollapse();\r\n\t\t\t\t\t\t$emit('click-item', item);\r\n\t\t\t\t\t}\r\n\t\t\t\t\"\r\n\t\t\t>\r\n\t\t\t\t<ItemSearch :item=\"item\" />\r\n\t\t\t</router-link>\r\n\t\t</SearchInput>\r\n\t\t<div class=\"Actionbar-title\" v-else>Customer</div>\r\n\r\n\t\t<ActionbarMenus\r\n\t\t\tclass=\"Actionbar-rightMenus\"\r\n\t\t\t:menus=\"[\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: 'Add',\r\n\t\t\t\t\ticon: host.res('icon/add-2A4858.svg'),\r\n\t\t\t\t\tclick: () => $emit('click-item-add'),\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\ttitle: 'Refresh',\r\n\t\t\t\t\ticon: host.res('icon/refresh-2A4858.svg'),\r\n\t\t\t\t\tclick: () => $emit('click-refresh'),\r\n\t\t\t\t},\r\n\t\t\t]\"\r\n\t\t/>\r\n\t</div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n\t.Actionbar {\r\n\t\tbackground-color: #e5ecee;\r\n\t\tdisplay: flex;\r\n\t\tfl