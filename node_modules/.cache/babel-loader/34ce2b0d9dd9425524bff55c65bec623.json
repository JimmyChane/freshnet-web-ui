";\r\n   import ActionbarProduct from \"@/pages/product/ActionBarProduct.vue\";\r\n   import DropdownSelector from \"@/components/selector/DropdownSelector/DropdownSelector.vue\";\r\n   import Empty from \"@/components/Empty.vue\";\r\n   import Footer from \"@/Footer.vue\";\r\n\r\n   export default {\r\n      components: {\r\n         LayoutProductBrowseProducts,\r\n         ActionbarProduct,\r\n         DropdownSelector,\r\n         Empty,\r\n         Footer,\r\n      },\r\n      emits: [\"click-drawer-expand\"],\r\n      data() {\r\n         return { scrollTop: 0 };\r\n      },\r\n      computed: {\r\n         user: (c) => c.loginStore.getters.user,\r\n         userType: (c) => (c.user ? c.user.type : User.Type.None),\r\n         isUserAdmin: (c) => c.userType === User.Type.Admin,\r\n         isUserStaff: (c) => c.userType === User.Type.Staff,\r\n         isEditable: (c) => c.isUserAdmin || c.isUserStaff,\r\n\r\n         groupingMenus: (c) => {\r\n            return Object.keys(ProductFilter.Filter)\r\n               .map((key) => ProductFilter.Filter[key])\r\n               .map((filter) => {\r\n                  return { key: filter.key, title: filter.title };\r\n               });\r\n         },\r\n\r\n         categoryKey: (c) => c.$route.params.category,\r\n         category: (c) =>\r\n            c.productStore.getters.categories.find((category) => {\r\n               return category.getKey() === c.categoryKey;\r\n            }),\r\n         categoryId: (c) => (c.category ? c.category.getId() : \"\"),\r\n         categoryIcon: (c) =>\r\n            c.category && c.category.getIcon() ? c.category.getIcon().url() : \"\",\r\n         categoryTitle: (c) => (c.category ? c.category.getTitle() : \"\"),\r\n\r\n         filterKey: (c) => c.$route.query.filter,\r\n\r\n         products: (c) => {\r\n            let products = c.productStore.getters.products.filter((product) => {\r\n               return product.getCategoryId() === c.categoryId;\r\n            });\r\n\r\n            if (!c.isEditable) {\r\n               products = products.filter((product) => {\r\n                  return product.isStockAvailable();\r\n               });\r\n            }\r\n\r\n            return products.sort((product1, product2) => product1.compare(product2));\r\n         },\r\n      },\r\n      mounted() {\r\n         this.productStore.dispatch(\"getProductCategories\");\r\n         this.productStore.dispatch(\"getProducts\");\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      class=\"PageProductCategory\"\r\n      @scroll=\"(event) => (scrollTop = event.target.scrollTop)\"\r\n   >\r\n      <ActionbarProduct\r\n         class=\"PageProductCategory-actionbar\"\r\n         :isParentScrolledDown=\"scrollTop > 0\"\r\n         :titleSearchPlaceholder=\"`Search ${categoryTitle.toLowerCase()}`\"\r\n         :products=\"products\"\r\n         @click-drawer-expand=\"$emit('click-drawer-expand')\"\r\n      />\r\n\r\n      <div class=\"PageProductCategory-header\">\r\n         <div class=\"PageProductCategory-title\">\r\n            <span class=\"PageProductCategory-text\" v-if=\"categoryTitle\">\r\n               {{ categoryTitle }}\r\n            </span>\r\n            <!-- <span class=\"PageProductCategory-productCount\">{{ products.length }}</span> -->\r\n         </div>\r\n\r\n         <div class=\"PageProductCategory-groupOption-item\" v-if=\"products.length\">\r\n            <span>Group By</span>\r\n            <DropdownSelector\r\n               :menus=\"groupingMenus\"\r\n               :currentMenuKey=\"filterKey\"\r\n               @menu-click=\"\r\n                  (menu) => {\r\n                     this.$root.replaceRoute({ query: { filter: menu.key } });\r\n                  }\r\n               \"\r\n            />\r\n         </div>\r\n      </div>\r\n\r\n      <div class=\"PageProductCategory-body\">\r\n         <LayoutProductBrowseProducts\r\n            class=\"PageProductCategory-products\"\r\n            :category=\"category\"\r\n            :products=\"products\"\r\n            @click-productAdd=\"(x) => $emit('click-productAdd', x)\"\r\n         />\r\n\r\n         <Empty class=\"PageProductCategory-empty\" v-if=\"!products.length\" />\r\n      </div>\r\n\r\n      <Footer />\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .PageProductCategory {\r\n      width: 100%;\r\n      height: 100%;\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      justify-content: flex-start;\r\n      position: relative;\r\n      overflow-y: auto;\r\n      background-color: var(--background-color);\r\n\r\n      .PageProductCategory-actionbar {\r\n         z-index: 3;\r\n      }\r\n\r\n      .PageProductCategory-header {\r\n         z-index: 2;\r\n         width: 100%;\r\n         display: flex;\r\n         flex-direction: row;\r\n         flex-wrap: wrap;\r\n         justify-content: space-between;\r\n         padding: 2em 1em;\r\n\r\n         .PageProductCategory-title {\r\n            width: max-content;\r\n            top: 0.4rem;\r\n            left: 0.8rem;\r\n            opacity: 1;\r\n            font-size: 4rem;\r\n            color: #2a4858;\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-wrap: wrap;\r\n            gap: 0.1em;\r\n            .PageProductCategory-text {\r\n               white-space: nowrap;\r\n               width: max-content;\r\n            }\r\n            .PageProductCategory-productCount {\r\n               font-size: 0.2em;\r\n               background: #2f579d;\r\n               color: white;\r\n               border-radius: 50%;\r\n               display: flex;\r\n               align-items: center;\r\n               justify-content: center;\r\n               padding-top: 0.1em;\r\n               --size: 2em;\r\n               width: var(--size);\r\n               height: var(--size);\r\n               min-width: var(--size);\r\n               min-height: var(--size);\r\n               max-width: var(--size);\r\n               max-height: var(--size);\r\n            }\r\n         }\r\n         .PageProductBrowseCategory-groupOption-item {\r\n            padding: 2rem 1.5rem;\r\n            min-width: 10em;\r\n            max-width: 100%;\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: flex-start;\r\n            justify-content: center;\r\n            gap: 0.5em;\r\n         }\r\n      }\r\n\r\n      .PageProductCategory-body {\r\n         z-index: 1;\r\n         width: 100%;\r\n         display: flex;\r\n         flex-grow: 1;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         .PageProductCategory-products {\r\n            z-index: 1;\r\n         }\r\n         .PageProductCategory-empty {\r\n            padding-top: 6em;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/product"}]}