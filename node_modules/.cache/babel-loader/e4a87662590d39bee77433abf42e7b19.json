AA,CAAA,kBAAA;AACA,UAAA,SAAA,GAAA,CAAA,CAAA,qBAAA;AAEA,UAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,SAAA,CAAA,QAAA,MAAA,CAAA,EAAA,OAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAAA;AACA,UAAA,MAAA,CAAA,QAAA,KAAA,CAAA,IAAA,SAAA,CAAA,QAAA,KAAA,CAAA,EACA,OAAA;AAAA,QAAA,IAAA,EAAA,MAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAAA;AACA,UAAA,MAAA,CAAA,QAAA,MAAA,CAAA,IAAA,SAAA,CAAA,QAAA,KAAA,CAAA,EACA,OAAA;AAAA,QAAA,IAAA,EAAA,MAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAAA;AACA,aAAA,IAAA;AACA,KA9DA;AAgEA,IAAA,WAAA,EAAA,qBAAA,CAAA;AAAA,aAAA,CAAA,CAAA,OAAA,CAAA,EAAA,GAAA,WAAA,EAAA;AAAA,KAhEA;AAiEA,IAAA,UAAA,EAAA,oBAAA,CAAA;AAAA,aAAA,CAAA,CAAA,OAAA,CAAA,EAAA,GAAA,UAAA,EAAA;AAAA;AAjEA,GANA;AAyEA,EAAA,OAzEA,qBAyEA;AACA,SAAA,YAAA,CAAA,QAAA,CAAA,UAAA;AACA;AA3EA,CAAA","sourcesContent":["<script>\r\n   import User from \"@/../../express/data_structures/User.js\";\r\n   import Setting from \"@/../../express/data_structures/Setting.js\";\r\n   import ProductPrice from \"@/items/ProductPrice.js\";\r\n\r\n   export default {\r\n      emits: [\"click\"],\r\n      props: {\r\n         item: { type: Object, default: () => null },\r\n         isSelected: { type: Boolean, default: false },\r\n      },\r\n      computed: {\r\n         user: (c) => c.userStore.getters.user,\r\n         userType: (c) => (c.user ? c.user.type : User.Type.None),\r\n         isUserAdmin: (c) => c.userType === User.Type.Admin,\r\n         isUserStaff: (c) => c.userType === User.Type.Staff,\r\n         isEditable: (c) => c.isUserAdmin || c.isUserStaff,\r\n\r\n         product: (c) => c.item,\r\n\r\n         productImages: (c) => c.product.getImages(),\r\n         thumbnail: (c) => (c.productImages.length ? c.productImages[0] : \"\"),\r\n\r\n         productTitle: (c) => (c.product ? c.product.getTitle() : \"\"),\r\n         productBrandId: (c) => (c.product ? c.product.getBrandId() : \"\"),\r\n         productBrand: (c) =>\r\n            c.brandStore.getters.items.find((brand) => {\r\n               return brand.getId() === c.productBrandId;\r\n            }),\r\n         brandTitle: (c) => (c.productBrand ? c.productBrand.getTitle() : \"\"),\r\n         fullTitle: (c) => {\r\n            let { productTitle, brandTitle } = c;\r\n            if (productTitle && brandTitle) return `${brandTitle} ${productTitle}`;\r\n            if (productTitle) return productTitle;\r\n            if (brandTitle) return brandTitle;\r\n            return \"\";\r\n         },\r\n\r\n         isAvailable: (c) => (c.product ? c.product.isStockAvailable() : false),\r\n\r\n         shouldShowPrice: (c) => {\r\n            let setting = c.settingStore.getters.items.find((setting) => {\r\n               return setting.key === Setting.Key.PublicShowPrice;\r\n            });\r\n            return setting ? setting.value : false;\r\n         },\r\n\r\n         productPrice: (c) => {\r\n            if (!c.isEditable && !c.shouldShowPrice) return null;\r\n            return c.product.getPrice();\r\n         },\r\n         productPriceNormal: (c) => {\r\n            return c.productPrice && c.productPrice.normal\r\n               ? c.productPrice.normal\r\n               : new ProductPrice();\r\n         },\r\n         productPricePromotion: (c) => {\r\n            return c.productPrice && c.productPrice.promotion\r\n               ? c.productPrice.promotion\r\n               : new ProductPrice();\r\n         },\r\n         price: (c) => {\r\n            if (!c.isEditable && !c.shouldShowPrice) return null;\r\n\r\n            let normal = c.productPriceNormal;\r\n            let promotion = c.productPricePromotion;\r\n\r\n            if (normal.getValue() > 0 && promotion.getValue() <= 0) return { to: normal };\r\n            if (normal.getValue() > 0 && promotion.getValue() > 0)\r\n               return { from: normal, to: promotion };\r\n            if (normal.getValue() <= 0 && promotion.getValue() > 0)\r\n               return { from: normal, to: promotion };\r\n            return null;\r\n         },\r\n\r\n         priceLabels: (c) => c.product.ui().stockLabels(),\r\n         specLabels: (c) => c.product.ui().specLabels(),\r\n      },\r\n      mounted() {\r\n         this.settingStore.dispatch(\"getItems\");\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      :class=\"[\r\n         'ItemProductVertical',\r\n         `ItemProductVertical-${productImages.length ? 'hasImages' : 'noImages'}`,\r\n         `ItemProductVertical-${isSelected ? 'isSelected' : 'isNotSelected'}`,\r\n         `ItemProductVertical-${isAvailable ? 'isAvailable' : 'isUnavailable'}`,\r\n      ]\"\r\n      :ref=\"product._id\"\r\n      @click=\"$emit('click', product)\"\r\n   >\r\n      <div class=\"ItemProductVertical-background\"></div>\r\n      <img\r\n         class=\"ItemProductVertical-imgBackground\"\r\n         v-if=\"thumbnail\"\r\n         :src=\"thumbnail.url({ width: 320 })\"\r\n      />\r\n      <div class=\"ItemProductVertical-foreground\"></div>\r\n      <div class=\"ItemProductVertical-gradient\"></div>\r\n\r\n      <div class=\"ItemProductVertical-main\">\r\n         <span class=\"ItemProductVertical-title\">{{ fullTitle }}</span>\r\n\r\n         <div class=\"ItemProductVertical-thumbnails\" v-if=\"productImages.length\">\r\n            <img\r\n               v-for=\"productImage of productImages\"\r\n               :key=\"productImage.url({ width: 50 })\"\r\n               :src=\"productImage.url({ width: 50 })\"\r\n            />\r\n         </div>\r\n\r\n         <div class=\"ItemProductVertical-specs\" v-if=\"specLabels.length\">\r\n            <span v-for=\"label in specLabels\" :key=\"label.text\">{{ label.text }}</span>\r\n         </div>\r\n      </div>\r\n\r\n      <div class=\"ItemProductVertical-highlight\"></div>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .ItemProductVertical {\r\n      --available-opacity: 1;\r\n\r\n      width: 100%;\r\n      transition: var(--animation-duration);\r\n      text-decoration: none;\r\n      font-size: 1.1rem;\r\n      background: white;\r\n      position: relative;\r\n      aspect-ratio: 16/12;\r\n      border: none;\r\n      border-radius: 0.8em;\r\n      overflow: hidden;\r\n      box-shadow: 0.1em 0.2em 0.5em rgba(73, 116, 137, 0.5);\r\n\r\n      display: flex;\r\n      flex-direction: column;\r\n      flex-wrap: nowrap;\r\n      align-items: flex-start;\r\n      justify-content: flex-end;\r\n\r\n      & > * {\r\n         position: absolute;\r\n         width: 100%;\r\n         height: 100%;\r\n      }\r\n\r\n      .ItemProductVertical-background {\r\n         z-index: 1;\r\n         background-color: var(--primary-color);\r\n         opacity: 0.4;\r\n      }\r\n      .ItemProductVertical-imgBackground {\r\n         z-index: 2;\r\n         object-fit: cover;\r\n         opacity: var(--available-opacity);\r\n      }\r\n      .ItemProductVertical-foreground {\r\n         z-index: 3;\r\n         background-color: var(--primary-color);\r\n         opacity: 0.9;\r\n      }\r\n      .ItemProductVertical-gradient {\r\n         z-index: 4;\r\n         background-image: linear-gradient(\r\n            0deg,\r\n            rgba(0, 0, 0, 1) 0%,\r\n            rgba(0, 0, 0, 0) 100%\r\n         );\r\n      }\r\n\r\n      .ItemProductVertical-main {\r\n         --padding: 1rem;\r\n\r\n         z-index: 5;\r\n         color: white;\r\n         opacity: var(--available-opacity);\r\n         transition: var(--animation-duration);\r\n         padding: var(--padding) 0.2rem;\r\n         gap: 0.2rem;\r\n\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: flex-start;\r\n         justify-content: flex-end;\r\n\r\n         & > * {\r\n            width: max-content;\r\n            max-width: 100%;\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-wrap: wrap;\r\n            gap: 0.2em;\r\n\r\n            padding: 0.2rem var(--padding);\r\n         }\r\n\r\n         .ItemProductVertical-title {\r\n            overflow-x: hidden;\r\n            white-space: nowrap;\r\n            line-height: 1;\r\n\r\n            font-size: 1.6em;\r\n            font-weight: 900;\r\n            text-align: start;\r\n         }\r\n         .ItemProductVertical-thumbnails {\r\n            flex-wrap: nowrap;\r\n            & > * {\r\n               border-radius: 0.2em;\r\n               height: 2.5em;\r\n               background: hsla(0, 0%, 100%, 0.8);\r\n            }\r\n         }\r\n         .ItemProductVertical-specs {\r\n            flex-wrap: nowrap;\r\n            & > * {\r\n               min-width: max-content;\r\n               width: max-content;\r\n               padding: 0.1em 0.4em;\r\n               color: black;\r\n               background: hsla(0, 0%, 100%, 0.8);\r\n               border-radius: 0.2em;\r\n               font-size: 0.6em;\r\n               font-weight: 900;\r\n            }\r\n         }\r\n      }\r\n\r\n      .ItemProductVertical-highlight {\r\n         z-index: 6;\r\n         background-color: transparent;\r\n         pointer-events: none;\r\n         transition: var(--animation-duration);\r\n      }\r\n   }\r\n\r\n   .ItemProductVertical-isNotSelected {\r\n      cursor: pointer;\r\n      &:hover,\r\n      &:focus,\r\n      &:focus-within {\r\n         .ItemProductVertical-main {\r\n            transform: scale(0.8) translateX(-13%) translateY(13%);\r\n         }\r\n         .ItemProductVertical-highlight {\r\n            background-color: hsla(0, 0%, 100%, 0.2);\r\n         }\r\n      }\r\n   }\r\n   .ItemProductVertical-isSelected {\r\n      .ItemProductVertical-main {\r\n         opacity: 0;\r\n         transform: scale(0.8) translateX(-13%) translateY(13%);\r\n      }\r\n      .ItemProductVertical-highlight {\r\n         background-color: hsla(0, 0%, 0%, 0.5);\r\n      }\r\n   }\r\n\r\n   .ItemProductVertical-isAvailable {\r\n      --available-opacity: 1;\r\n   }\r\n   .ItemProductVertical-isUnavailable {\r\n      --available-opacity: 0.1;\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/product/item"}]}