n               border-radius: 0.5rem;\r\n               padding: 0.5rem;\r\n               padding: 0.2rem;\r\n               overflow: hidden;\r\n               display: flex;\r\n               flex-direction: row;\r\n               flex-wrap: wrap;\r\n               align-items: flex-start;\r\n               column-gap: 2rem;\r\n               row-gap: 0.3rem;\r\n               .PanelRight-customer-body {\r\n                  flex-grow: 1;\r\n                  display: flex;\r\n                  flex-direction: column;\r\n                  align-items: flex-start;\r\n                  gap: 0.5rem;\r\n                  padding: 0 0.5rem;\r\n                  .PanelRight-customer-section {\r\n                     flex-grow: 0;\r\n                     width: 100%;\r\n                     display: flex;\r\n                     flex-direction: column;\r\n                     align-items: flex-start;\r\n                     line-height: 1.2;\r\n                     .PanelRight-customer-section-header {\r\n                        word-break: break-all;\r\n                        text-overflow: clip;\r\n                        flex-grow: 0;\r\n                        display: flex;\r\n                        flex-direction: row;\r\n                        align-items: center;\r\n                     }\r\n                     .PanelRight-customer-section-content {\r\n                        flex-grow: 1;\r\n                        display: flex;\r\n                        flex-direction: row;\r\n                        align-items: center;\r\n                     }\r\n                  }\r\n               }\r\n            }\r\n            .PanelRight-description {\r\n               background: hsla(201, 35%, 25%, 0.02);\r\n               border: 1px solid hsla(201, 35%, 25%, 0.05);\r\n               border: 0.1rem solid hsla(0, 0%, 0%, 0.2);\r\n               padding: 0.5rem;\r\n               padding: 0.2rem;\r\n               border-radius: 0.5rem;\r\n               dis