deviceIds.length) return;\r\n\r\n            const cacheItem = this.item;\r\n            this.isLoadingDevices = true;\r\n            this.customerStore.dispatch(\"getDevices\").then((devices) => {\r\n               if (this.item !== cacheItem) return;\r\n               this.isLoadingDevices = false;\r\n               this.devices = this.item.deviceIds.map((deviceId) => {\r\n                  const device = devices.find((device) => device._id === deviceId);\r\n                  return { deviceId, device };\r\n               });\r\n            });\r\n         },\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      class=\"PanelRight\"\r\n      v-if=\"item\"\r\n      @scroll=\"(event) => (top.showShadow = event.target.scrollTop > 0)\"\r\n   >\r\n      <div class=\"PanelRight-body\">\r\n         <div\r\n            class=\"PanelRight-top\"\r\n            :class=\"[top.showShadow ? 'PanelRight-top-showShadow' : '']\"\r\n         >\r\n            <ButtonIcon\r\n               :src=\"host.res('icon/close-2A4858.svg')\"\r\n               @click=\"$emit('click-item-close', { item })\"\r\n            />\r\n         </div>\r\n\r\n         <div class=\"PanelRight-main\">\r\n            <div class=\"PanelRight-customer\">\r\n               <div class=\"PanelRight-customer-body\">\r\n                  <div class=\"PanelRight-customer-section\">\r\n                     <span\r\n                        class=\"PanelRight-reuse-sectionTitle PanelRight-customer-section-header\"\r\n                        >Name</span\r\n                     >\r\n                     <span class=\"PanelRight-customer-section-content\" v-if=\"item.name\">{{\r\n                        item.name\r\n                     }}</span>\r\n                     <span\r\n                        class=\"PanelRight-reuse-contentEmpty PanelRight-customer-section-content\"\r\n                        v-else\r\n                        >Empty</span\r\n                     >\r\n                  </div>\