A;AApGA,CAAA","sourcesContent":["<script>\r\n   import ModuleEvent from \"@/../../express/data_structures/ServiceEvent.js\";\r\n   import TypeSelector from \"@/components/selector/TypeSelector.vue\";\r\n   import Window from \"@/components/window/Window.vue\";\r\n   import Input from \"@/components/Input.vue\";\r\n\r\n   export default {\r\n      components: { Window, TypeSelector, Input },\r\n      emits: [\"callback-create\", \"callback-cancel\"],\r\n      data() {\r\n         return {\r\n            ModuleEvent,\r\n\r\n            nameOfUser: \"unknown\",\r\n            eventMethod: ModuleEvent.Method.Info,\r\n\r\n            eventDescription: \"\",\r\n            eventStatus: \"\",\r\n            eventAmount: \"\",\r\n         };\r\n      },\r\n      computed: {\r\n         user: (c) => c.loginStore.getters.user,\r\n         isUserDefault: (c) => {\r\n            if (!c.user) return false;\r\n            const isUserAdmin = c.user.isTypeAdmin() && c.user.username === \"admin\";\r\n            const isUserStaff = c.user.isTypeStaff() && c.user.username === \"staff\";\r\n            return isUserAdmin || isUserStaff;\r\n         },\r\n         isMethodInfo: (c) => c.eventMethod === ModuleEvent.Method.Info,\r\n         isMethodQuotation: (c) => c.eventMethod === ModuleEvent.Method.Quotation,\r\n         isMethodPurchase: (c) => c.eventMethod === ModuleEvent.Method.Purchase,\r\n         nameUserType: (c) => {\r\n            if (!c.user) return \"\";\r\n            if (c.user.isTypeAdmin()) return \"admin\";\r\n            if (c.user.isTypeStaff()) return \"staff\";\r\n            return \"unknown\";\r\n         },\r\n      },\r\n      watch: {\r\n         user() {\r\n            let user = this.user;\r\n            if (user) {\r\n               this.nameOfUser = this.isUserDefault ? \"\" : user.name;\r\n            }\r\n         },\r\n         nameOfUser() {\r\n            if (this.nameOfUser.includes(\" \"))\r\n               this.nameOfUser = this.nameOfUser.trim().replace(\" \", \"\");\r\n         },\r\n      },\r\n      methods: {\r\n         reset() {\r\n            this.nameOfUser = \"\";\r\n            this.eventMethod = ModuleEvent.Method.Info;\r\n\r\n            this.eventDescription = \"\";\r\n            this.eventStatus = \"\";\r\n            this.eventAmount = \"\";\r\n         },\r\n\r\n         toEvent() {\r\n            let event = {\r\n               timestamp: Date.now(),\r\n               method: this.eventMethod,\r\n               description: this.eventDescription,\r\n            };\r\n\r\n            if (this.isMethodInfo) {\r\n               event.status = this.eventStatus;\r\n               return event;\r\n            }\r\n\r\n            if (this.isMethodQuotation || this.isMethodPurchase) {\r\n               event.price = {\r\n                  amount: this.eventAmount,\r\n                  currency: \"RM\",\r\n               };\r\n               return event;\r\n            }\r\n\r\n            return null;\r\n         },\r\n         submit() {\r\n            if (this.isUserDefault && !this.nameOfUser.trim()) {\r\n               this.$root.feedback(\"You must specify your name\");\r\n               return;\r\n            }\r\n            if (!this.eventDescription.trim()) {\r\n               this.$root.feedback('You must specify \"Description\"');\r\n               return;\r\n            }\r\n\r\n            let event = this.toEvent();\r\n            if (this.isUserDefault && this.nameOfUser.trim()) {\r\n               event.nameOfUser = this.nameOfUser;\r\n            }\r\n\r\n            if (event) {\r\n               this.$emit(\"callback-create\", event);\r\n               this.reset();\r\n            }\r\n         },\r\n      },\r\n      mounted() {\r\n         this.reset();\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <Window\r\n      class=\"WindowEvent\"\r\n      title=\"New Event\"\r\n      @click-cancel=\"\r\n         $emit('callback-cancel');\r\n         reset();\r\n      \"\r\n      @click-ok=\"submit()\"\r\n   >\r\n      <div class=\"WindowEvent-body\">\r\n         <div class=\"WindowEvent-user\">\r\n            <span class=\"WindowEvent-user-type\">{{ nameUserType }}</span>\r\n\r\n            <span class=\"WindowEvent-user-name-text\" v-if=\"!isUserDefault\">{{\r\n               nameOfUser\r\n            }}</span>\r\n\r\n            <Input\r\n               class=\"WindowEvent-user-name-input WindowEvent-inputText\"\r\n               v-if=\"isUserDefault\"\r\n               label=\"Your name here\"\r\n               :isRequired=\"true\"\r\n               :bindValue=\"nameOfUser\"\r\n               @input=\"(comp) => (nameOfUser = comp.value)\"\r\n            />\r\n         </div>\r\n\r\n         <div class=\"WindowEvent-type\">\r\n            <span class=\"WindowEvent-title\">Select Type</span>\r\n            <TypeSelector\r\n               class=\"WindowEvent-type-list\"\r\n               :items=\"[\r\n                  { key: ModuleEvent.Method.Info, title: 'Info', color: '#0771d2' },\r\n                  {\r\n                     key: ModuleEvent.Method.Quotation,\r\n                     title: 'Quotation',\r\n                     color: '#961d96',\r\n                  },\r\n                  {\r\n                     key: ModuleEvent.Method.Purchase,\r\n                     title: 'Purchase',\r\n                     color: '#258915',\r\n                  },\r\n               ]\"\r\n               :defaultKey=\"ModuleEvent.Method.Info\"\r\n               @click-item-key=\"(key) => (eventMethod = key)\"\r\n            />\r\n         </div>\r\n\r\n         <div class=\"WindowEvent-description\">\r\n            <span class=\"WindowEvent-title\">Description</span>\r\n            <textarea\r\n               class=\"WindowEvent-inputText\"\r\n               type=\"text\"\r\n               v-model=\"eventDescription\"\r\n            />\r\n            <span class=\"WindowEvent-required\">Required</span>\r\n         </div>\r\n\r\n         <Input\r\n            class=\"WindowEvent-status WindowEvent-inputText\"\r\n            v-if=\"isMethodInfo\"\r\n            label=\"Status\"\r\n            :bindValue=\"eventStatus\"\r\n            @input=\"(comp) => (eventStatus = comp.value)\"\r\n         />\r\n\r\n         <div\r\n            class=\"WindowEvent-amount\"\r\n            v-else-if=\"isMethodQuotation || isMethodPurchase\"\r\n         >\r\n            <span class=\"WindowEvent-title\">Price</span>\r\n            <div class=\"WindowEvent-amount-value\">\r\n               <span class=\"WindowEvent-amount-value-currency\">RM</span>\r\n               <input class=\"WindowEvent-inputText\" type=\"text\" v-model=\"eventAmount\" />\r\n            </div>\r\n         </div>\r\n      </div>\r\n   </Window>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   // Abstract\r\n   .WindowEvent-title {\r\n      font-size: 1.1rem;\r\n      font-weight: 900;\r\n   }\r\n   // Abstract\r\n   .WindowEvent-inputText {\r\n      font-size: 1rem;\r\n      border: none;\r\n      background: hsla(0, 0%, 0%, 0.03);\r\n      border-bottom: 1px solid hsl(0, 0%, 70%);\r\n      border-radius: 0.2rem;\r\n      padding: 0.6rem 0.4rem;\r\n      resize: none;\r\n   }\r\n   // Abstract\r\n   .WindowEvent-required {\r\n      color: hsl(0, 50%, 50%);\r\n      font-size: 0.8rem;\r\n   }\r\n\r\n   .WindowEvent {\r\n      max-width: 100%;\r\n      width: 35rem;\r\n      .WindowEvent-body {\r\n         font-size: 1rem;\r\n         font-weight: 400;\r\n         color: black;\r\n         display: flex;\r\n         flex-direction: column;\r\n         gap: 40px;\r\n\r\n         .WindowEvent-user {\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: flex-start;\r\n\r\n            .WindowEvent-user-type {\r\n               font-size: 0.9rem;\r\n               font-weight: 400;\r\n               color: hsl(0, 0%, 50%);\r\n            }\r\n            .WindowEvent-user-name-input {\r\n               width: 12rem;\r\n            }\r\n         }\r\n\r\n         .WindowEvent-type {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n            &-list {\r\n               width: 100%;\r\n            }\r\n         }\r\n\r\n         .WindowEvent-description {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n            .WindowEvent-inputText {\r\n               height: 6rem;\r\n            }\r\n         }\r\n\r\n         .WindowEvent-amount {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n            &-value {\r\n               width: 100%;\r\n               display: flex;\r\n               flex-direction: row;\r\n               align-items: stretch;\r\n               gap: 10px;\r\n               &-currency {\r\n                  display: flex;\r\n                  flex-direction: row;\r\n                  align-items: center;\r\n                  border-bottom: 1px solid transparent;\r\n               }\r\n               .WindowEvent-inputText {\r\n                  flex-grow: 1;\r\n               }\r\n            }\r\n         }\r\n\r\n         .WindowEvent-result {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/service/windows"}]}