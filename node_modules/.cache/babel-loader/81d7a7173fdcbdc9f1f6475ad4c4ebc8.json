getId() {\r\n      return this.data._id;\r\n   }\r\n   getTitle() {\r\n      return this.data.title;\r\n   }\r\n   getIcon() {\r\n      return this.data.icon;\r\n   }\r\n\r\n   isEqual(obj) {\r\n      return this._id == obj._id && this.title == obj.title;\r\n   }\r\n\r\n   // legacies\r\n   get _id() {\r\n      return this.getId();\r\n   }\r\n   get title() {\r\n      return this.getTitle();\r\n   }\r\n   get icon() {\r\n      return this.getIcon();\r\n   }\r\n}\r\n\r\nexport default ProductBrand;\r\n"]}]}