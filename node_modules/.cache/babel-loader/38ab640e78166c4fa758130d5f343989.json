AA,CAAA,gBAAA,EADA;AAAA,OAAA,CADA;AAAA,KARA;AAaA,IAAA,SAAA,EAAA,mBAAA,CAAA;AAAA,aAAA,KAAA,CAAA,OAAA,CAAA,CAAA,CAAA,UAAA,KAAA,CAAA,CAAA,UAAA,CAAA,MAAA;AAAA,KAbA;AAeA,IAAA,qBAAA,EAAA,+BAAA,CAAA;AAAA,aAAA,CAAA;AAAA,KAfA;AAgBA,IAAA,cAAA,EAAA,wBAAA,CAAA,EAAA;AACA,UAAA,CAAA,CAAA,QAAA,CAAA,MAAA,IAAA,CAAA,CAAA,qBAAA,EAAA;AACA,eAAA,CAAA,CAAA,QAAA;AACA;;AAEA,UAAA,cAAA,GAAA,EAAA;;AAEA,aAAA,cAAA,CAAA,MAAA,GAAA,CAAA,CAAA,qBAAA,EAAA;AACA,YAAA,WAAA,GAAA,IAAA,CAAA,MAAA,MAAA,CAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA,QAAA,WAAA,GAAA,MAAA,CAAA,QAAA,CAAA,WAAA,CAAA;AACA,YAAA,aAAA,GAAA,CAAA,CAAA,QAAA,CAAA,WAAA,CAAA;;AACA,YAAA,CAAA,cAAA,CAAA,QAAA,CAAA,aAAA,CAAA,EAAA;AACA,UAAA,cAAA,CAAA,IAAA,CAAA,aAAA;AACA;AACA;;AAEA,aAAA,cAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,SAAA,GAAA,MAAA,GAAA,QAAA,CAAA,SAAA,GAAA,MAAA;AACA,OAFA,CAAA;AAGA;AAnCA,GATA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,SAAA,YAAA,CAAA,QAAA,CAAA,8BAAA;AACA;AAhDA,CAAA","sourcesContent":["<script>\r\n   import PageHomeSection from \"./PageHome_Section.vue\";\r\n   import SectionProductCategoryItem from \"./SectionProductCategoryItem.vue\";\r\n   import SectionProductProductItem from \"./SectionProductProductItem.vue\";\r\n\r\n   export default {\r\n      components: {\r\n         PageHomeSection,\r\n         SectionProductCategoryItem,\r\n         SectionProductProductItem,\r\n      },\r\n      props: {\r\n         isParentThin: { type: Boolean, default: false },\r\n      },\r\n      computed: {\r\n         categories: (c) =>\r\n            c.productStore.getters.groups\r\n               .filter(\r\n                  (group) => group.items.filter((item) => item.isStockAvailable()).length\r\n               )\r\n               .filter((group) => group.items.length)\r\n               .sort((category1, category2) => category1.compare(category2)),\r\n         products: (c) =>\r\n            c.productStore.getters.products.filter((product) =>\r\n               product.isStockAvailable()\r\n            ),\r\n\r\n         hasGroups: (c) => Array.isArray(c.categories) && c.categories.length,\r\n\r\n         randomProductMaxCount: (c) => 5,\r\n         randomProducts: (c) => {\r\n            if (c.products.length <= c.randomProductMaxCount) {\r\n               return c.products;\r\n            }\r\n\r\n            let randomProducts = [];\r\n\r\n            while (randomProducts.length < c.randomProductMaxCount) {\r\n               let randomIndex = Math.random() * (c.products.length - 1);\r\n               randomIndex = Number.parseInt(randomIndex);\r\n               let randomProduct = c.products[randomIndex];\r\n               if (!randomProducts.includes(randomProduct)) {\r\n                  randomProducts.push(randomProduct);\r\n               }\r\n            }\r\n\r\n            return randomProducts.sort((product1, product2) => {\r\n               return product2.getImages().length - product1.getImages().length;\r\n            });\r\n         },\r\n      },\r\n      mounted() {\r\n         this.productStore.dispatch(\"getProductsGroupByCategories\");\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <PageHomeSection\r\n      class=\"HomeSectionProduct\"\r\n      :isParentThin=\"isParentThin\"\r\n      title=\"What We Sell\"\r\n      v-if=\"categories.length\"\r\n   >\r\n      <div\r\n         class=\"HomeSectionProduct-box\"\r\n         :class=\"[\r\n            `HomeSectionProduct-box-${hasGroups ? 'hasGroups' : 'hasNoCategories'}`,\r\n         ]\"\r\n      >\r\n         <div class=\"HomeSectionProduct-body\">\r\n            <div class=\"HomeSectionProduct-section HomeSectionProduct-categories\">\r\n               <!-- <span class=\"HomeSectionProduct-section-title\">Categories</span> -->\r\n               <div class=\"HomeSectionProduct-categories-body\">\r\n                  <SectionProductCategoryItem\r\n                     class=\"HomeSectionProduct-categories-item\"\r\n                     v-for=\"category in categories\"\r\n                     :key=\"category.getKey()\"\r\n                     :category=\"category\"\r\n                  />\r\n               </div>\r\n            </div>\r\n\r\n            <div class=\"HomeSectionProduct-separator\"></div>\r\n\r\n            <div class=\"HomeSectionProduct-section HomeSectionProduct-products\">\r\n               <!-- <span class=\"HomeSectionProduct-section-title\">Recommended</span> -->\r\n               <div class=\"HomeSectionProduct-products-body\">\r\n                  <SectionProductProductItem\r\n                     v-for=\"product of randomProducts\"\r\n                     :key=\"product.getId()\"\r\n                     :product=\"product\"\r\n                  />\r\n               </div>\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"HomeSectionProduct-empty\">\r\n            <p>No Products</p>\r\n         </div>\r\n      </div>\r\n   </PageHomeSection>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .HomeSectionProduct {\r\n      --padding: 0;\r\n      .HomeSectionProduct-box {\r\n         width: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: center;\r\n         justify-content: center;\r\n         padding: 2em 0;\r\n         background: white;\r\n         .HomeSectionProduct-body {\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-wrap: wrap;\r\n            align-items: center;\r\n            justify-content: space-evenly;\r\n            gap: 1rem;\r\n            gap: 2em;\r\n\r\n            .HomeSectionProduct-separator {\r\n               min-height: 0.02em;\r\n               width: 100%;\r\n               background: hsla(0, 0%, 0%, 0.4);\r\n               margin: 0 2em;\r\n            }\r\n            .HomeSectionProduct-section {\r\n               width: 100%;\r\n               display: flex;\r\n               flex-direction: column;\r\n               align-items: flex-start;\r\n               justify-content: flex-start;\r\n               gap: 0.7em;\r\n               .HomeSectionProduct-section-title {\r\n                  font-size: 1.2em;\r\n                  font-weight: 900;\r\n                  padding: 0 2em;\r\n               }\r\n            }\r\n\r\n            .HomeSectionProduct-categories {\r\n               .HomeSectionProduct-categories-body {\r\n                  width: 100%;\r\n                  display: flex;\r\n                  flex-direction: row;\r\n                  flex-wrap: wrap;\r\n                  align-items: center;\r\n                  justify-content: flex-start;\r\n                  gap: 0.4em;\r\n                  gap: 0.2em;\r\n                  padding: 0 2em;\r\n               }\r\n            }\r\n            .HomeSectionProduct-products {\r\n               .HomeSectionProduct-products-body {\r\n                  width: 100%;\r\n                  display: flex;\r\n                  flex-direction: row;\r\n                  align-items: stretch;\r\n                  justify-content: flex-start;\r\n                  gap: 0.4em;\r\n                  overflow-x: auto;\r\n                  padding: 0 2em;\r\n                  padding-bottom: 0.5em;\r\n\r\n                  --scrollbar-size: 0.2em;\r\n                  --scrollbar-thumb-color: hsla(209, 43%, 46%, 0.2);\r\n                  --scrollbar-thumb-color-hover: hsla(209, 43%, 46%, 0.4);\r\n                  --scrollbar-track-margin: 2em;\r\n                  --scrollbar-track-color: none;\r\n                  --scrollbar-track-color-hover: none;\r\n\r\n                  --scrollbar-size: 0;\r\n               }\r\n            }\r\n         }\r\n         .HomeSectionProduct-empty {\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: center;\r\n            justify-content: center;\r\n            opacity: 0.4;\r\n         }\r\n      }\r\n\r\n      .HomeSectionProduct-box-hasGroups {\r\n         .HomeSectionProduct-body {\r\n            display: flex;\r\n         }\r\n         .HomeSectionProduct-empty {\r\n            display: none;\r\n         }\r\n      }\r\n      .HomeSectionProduct-box-hasNoCategories {\r\n         .HomeSectionProduct-body {\r\n            display: none;\r\n         }\r\n         .HomeSectionProduct-empty {\r\n            display: flex;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/home"}]}