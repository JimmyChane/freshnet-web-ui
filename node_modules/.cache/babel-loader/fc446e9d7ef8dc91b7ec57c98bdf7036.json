r\n         description: (context) => context.item.description,\r\n         images: (context) => context.item.getImageFiles(),\r\n         isUrgent: (context) => context.item.isUrgent(),\r\n         isWarranty: (context) => context.item.isWarranty(),\r\n         events: (context) => {\r\n            const events = context.item.getEvents();\r\n            return (Array.isArray(events) ? events : [])\r\n               .map((event) => event)\r\n               .sort((event1, event2) => event1.compare(event2));\r\n         },\r\n         totalCost: (context) => {\r\n            return context.events.reduce((cost, event) => {\r\n               if (event.method === EventModule.Method.Purchase) {\r\n                  cost = cost.plus(event.price);\r\n               }\r\n               return cost;\r\n            }, new ServicePrice(0));\r\n         },\r\n         totalCostAmount: (context) =>\r\n            context.totalCost ? context.totalCost.getAmount() : 0,\r\n         timestamp: (context) => context.item.timestamp,\r\n\r\n         state: (context) => context.item.state,\r\n         primaryColor: (context) => ServiceState.getResourceByKey(context.state).color,\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <Button3\r\n      class=\"ItemServiceGrid\"\r\n      :style=\"{ '--primary-color': primaryColor }\"\r\n      :isSelected=\"isSelected\"\r\n      @click=\"$emit('callback-view', item)\"\r\n   >\r\n      <div\r\n         :class=\"[\r\n            'ItemServiceGrid-body',\r\n            `ItemServiceGrid-body-${isSelected ? 'isSelected' : 'isDeselected'}`,\r\n         ]\"\r\n      >\r\n         <div class=\"ItemServiceGrid-top\">\r\n            <div class=\"ItemServiceGrid-top-customer\">\r\n               <span class=\"ItemServiceGrid-top-customer-name\">{{ name }}</span>\r\n               <span class=\"ItemServiceGrid-top-customer-phoneNumber\">{{\r\n                  phoneNumber\r\n               }}</span>\r\n            </div>\r\n\r\n            <div class=\"ItemServiceGrid-top-dot-body\">\r\n               <div class=\"ItemServiceGrid-state-dot\"></div>\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"ItemServiceGrid-middle\">\r\n            <div class=\"ItemServiceGrid-images\">\r\n               <img\r\n                  class=\"ItemServiceGrid-image\"\r\n                  v-for=\"image of images\"\r\n                  :key=\"image.name\"\r\n                  :src=\"host.imgFile.name(image.name, { height: 30 })\"\r\n               />\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"ItemServiceGrid-bottom\">\r\n            <span class=\"ItemServiceGrid-timestamp\">{{ timestamp }}</span>\r\n         </div>\r\n      </div>\r\n   </Button3>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .ItemServiceGrid {\r\n      width: 100%;\r\n      background: white;\r\n\r\n      .ItemServiceGrid-body {\r\n         width: 100%;\r\n         height: 100%;\r\n         font-weight: 400;\r\n         font-size: 1em;\r\n         color: black;\r\n         text-align: start;\r\n         line-height: 1.1;\r\n         gap: 0.5rem;\r\n         padding: 0.6rem 0.2rem;\r\n\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         transition: var(--animation-duration);\r\n         border-radius: 0 0.5em 0.5em 0;\r\n         border: 0.1em solid transparent;\r\n\r\n         .ItemServiceGrid-top {\r\n            padding-bottom: 0.6rem;\r\n            border-bottom: 0.05rem solid hsla(0, 0%, 0%, 0.1);\r\n            margin-right: 0.6rem;\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: flex-start;\r\n            .ItemServiceGrid-top-customer {\r\n               font-weight: 900;\r\n\r\n               display: flex;\r\n               flex-direction: column;\r\n               flex-grow: 1;\r\n               align-items: flex-start;\r\n               justify-content: flex-start;\r\n            }\r\n            .ItemServiceGrid-top-dot-body {\r\n               display: flex;\r\n               flex-direction: column;\r\n               align-items: center;\r\n               justify-content: flex-end;\r\n               .ItemServiceGrid-state-dot {\r\n                  --size: 0.5em;\r\n                  min-width: var(--size);\r\n                  min-height: var(--size);\r\n                  width: var(--size);\r\n                  height: var(--size);\r\n                  background: var(--primary-color);\r\n                  border-radius: var(--size);\r\n               }\r\n            }\r\n         }\r\n         .ItemServiceGrid-middle {\r\n            width: 100%;\r\n            height: 100%;\r\n            gap: 0.7em;\r\n            font-weight: 400;\r\n            font-size: 1em;\r\n            color: black;\r\n            text-align: start;\r\n            line-height: 1.1;\r\n            overflow: hidden;\r\n\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-direction: column;\r\n            flex-wrap: wrap;\r\n            flex-grow: 1;\r\n            align-items: flex-start;\r\n            justify-content: space-between;\r\n\r\n            .ItemServiceGrid-images {\r\n               flex-grow: 0;\r\n               display: flex;\r\n               flex-direction: row;\r\n               flex-wrap: wrap;\r\n               align-items: flex-start;\r\n               justify-content: flex-start;\r\n               gap: 0.2em;\r\n               overflow: hidden;\r\n               .ItemServiceGrid-image {\r\n                  height: 40px;\r\n               }\r\n            }\r\n         }\r\n         .ItemServiceGrid-bottom {\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: flex-end;\r\n            justify-content: flex-start;\r\n            gap: 1rem;\r\n\r\n            .ItemServiceGrid-timestamp {\r\n               font-size: 0.8em;\r\n               color: rgb(112, 112, 112);\r\n               text-align: start;\r\n            }\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/service/items"}]}