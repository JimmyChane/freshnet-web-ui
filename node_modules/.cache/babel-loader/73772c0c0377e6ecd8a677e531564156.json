AA,OAAA,MAAA,CAAA,IAAA,CAAA,eAAA;AACA,UAAA,CAAA,KAAA,QAAA,EAAA,OAAA,MAAA,CAAA,IAAA,CAAA,KAAA;AACA,aAAA,CAAA;AACA,KAPA;AAQA,IAAA,UAAA,EAAA;AAAA,aAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAAA;AARA,GAbA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,WAAA,KAAA,CAAA,gBAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA,KAJA;AAMA,IAAA,eANA,2BAMA,IANA,EAMA;AACA,aACA,KAAA,eAAA,KAAA,IAAA,CAAA,GAAA,IACA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,CADA,IAEA,IAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAFA,IAGA,KAAA,KAAA,CAAA,OAAA,CAAA,IAAA,MAAA,CAJA;AAMA;AAbA;AAvBA,CAAA","sourcesContent":["<script>\r\n   import Drawer from \"@/components/Drawer.vue\";\r\n   import LeftNavigationPage from \"./LeftNavigation_Page.vue\";\r\n   import LeftNavigationLogin from \"./LeftNavigation_Login.vue\";\r\n\r\n   export default {\r\n      components: { Drawer, LeftNavigationPage, LeftNavigationLogin },\r\n      emits: [\"click-collapse\", \"click-logout\"],\r\n      props: {\r\n         isExpand: { type: Boolean, default: false },\r\n         isDrawer: { type: Boolean, default: true },\r\n         isWide: { type: Boolean, default: false },\r\n         selectedPageKey: { type: String, default: \"\" },\r\n         selectedViewKey: { type: String, default: \"\" },\r\n      },\r\n      data() {\r\n         return { expandedPageKey: \"\" };\r\n      },\r\n      computed: {\r\n         pages: (c) => c.$root.pages,\r\n         drawerMode() {\r\n            if (this.isDrawer && this.isExpand) return Drawer.Mode.DRAWER_EXPAND;\r\n            if (this.isDrawer && !this.isExpand) return Drawer.Mode.DRAWER_COLLAPSE;\r\n            if (!this.isDrawer) return Drawer.Mode.FIXED;\r\n            return 0;\r\n         },\r\n         drawerEdge: () => Drawer.Edge.LEFT,\r\n      },\r\n      methods: {\r\n         emitCollapse() {\r\n            this.$emit(\"click-collapse\");\r\n            this.expandedPageKey = \"\";\r\n         },\r\n\r\n         shouldMakeSpace(page) {\r\n            return (\r\n               this.selectedPageKey === page.key &&\r\n               Array.isArray(page.sections) &&\r\n               page.sections.length > 0 &&\r\n               this.pages.indexOf(page) !== 0\r\n            );\r\n         },\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <Drawer\r\n      :class=\"['LeftNavigation', `LeftNavigation-${isWide ? 'isWide' : 'isThin'}`]\"\r\n      :mode=\"drawerMode\"\r\n      :edge=\"drawerEdge\"\r\n      @click-collapse=\"emitCollapse()\"\r\n   >\r\n      <div class=\"LeftNavigation-body\">\r\n         <div class=\"LeftNavigation-pages\" v-if=\"pages\">\r\n            <LeftNavigationPage\r\n               :class=\"[shouldMakeSpace(page) ? 'LeftNavigation-page-marginTop' : '']\"\r\n               v-for=\"page of pages\"\r\n               :key=\"page.key\"\r\n               :page=\"page\"\r\n               :isExpanded=\"expandedPageKey === page.key\"\r\n               :isSelected=\"selectedPageKey === page.key\"\r\n               :isWide=\"isWide\"\r\n               :selectedViewKey=\"selectedViewKey\"\r\n               @click-page=\"emitCollapse()\"\r\n               @click-page-expand=\"\r\n                  () => (expandedPageKey = expandedPageKey === page.key ? '' : page.key)\r\n               \"\r\n               @click-page-section-view=\"emitCollapse()\"\r\n            />\r\n         </div>\r\n\r\n         <LeftNavigationLogin\r\n            v-if=\"$root.currentPageKey !== 'login'\"\r\n            @click-logout=\"$emit('click-logout')\"\r\n            :isWide=\"isWide\"\r\n         />\r\n      </div>\r\n   </Drawer>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .LeftNavigation {\r\n      .LeftNavigation-body {\r\n         width: 16rem;\r\n         height: 100%;\r\n         max-width: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         overflow-y: auto;\r\n         overflow-x: hidden;\r\n         background: #dfe8ee;\r\n         position: relative;\r\n         transition: var(--animation-duration);\r\n         .LeftNavigation-pages {\r\n            flex-grow: 1;\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: flex-start;\r\n            padding: 0.4em 0;\r\n            padding-bottom: 4em;\r\n\r\n            .LeftNavigation-page-marginTop {\r\n               margin-top: 1em;\r\n            }\r\n         }\r\n         .LeftNavigation-login {\r\n            position: sticky;\r\n            bottom: 0;\r\n            width: 100%;\r\n            box-shadow: 0px 0px 20px #49748940;\r\n         }\r\n      }\r\n   }\r\n\r\n   .LeftNavigation-isWide {\r\n      .LeftNavigation-body {\r\n         .LeftNavigation-pages {\r\n            padding: 0.8em;\r\n            padding-top: 0.8em;\r\n            padding-bottom: 4em;\r\n            align-items: flex-start;\r\n         }\r\n      }\r\n   }\r\n   .LeftNavigation-isThin {\r\n      .LeftNavigation-body {\r\n         width: fit-content;\r\n         .LeftNavigation-pages {\r\n            align-items: center;\r\n            padding: 0.3em;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/app"}]}