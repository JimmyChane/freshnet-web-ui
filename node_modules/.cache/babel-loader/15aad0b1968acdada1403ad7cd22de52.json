\n\r\n// testing\r\nconst Keyword = {\r\n   Processor: [\r\n      { title: \"Intel Pentium\", matches: [\"intelpentium\"] },\r\n      { title: \"Intel Celeron\", matches: [\"intelceleron\"] },\r\n      { title: \"Intel i3\", matches: [\"intelcorei3\", \"inteli3\"] },\r\n      { title: \"Intel i5\", matches: [\"intelcorei5\", \"inteli5\"] },\r\n      { title: \"Intel i7\", matches: [\"intelcorei7\", \"inteli7\"] },\r\n      { title: \"Intel i9\", matches: [\"intelcorei9\", \"inteli9\"] },\r\n      { title: \"AMD Ryzen 3\", matches: [\"amdryzen3\"] },\r\n      { title: \"AMD Ryzen 5\", matches: [\"amdryzen5\"] },\r\n      { title: \"AMD Ryzen 7\", matches: [\"amdryzen7\"] },\r\n   ],\r\n   Ram: [\r\n      { title: \"DDR3 2GB\", matches: [\"ddr32gb\"] },\r\n      { title: \"DDR3 4GB\", matches: [\"ddr34gb\"] },\r\n      { title: \"DDR3 8GB\", matches: [\"ddr38gb\"] },\r\n      { title: \"DDR3 16GB\", matches: [\"ddr316gb\"] },\r\n      { title: \"DDR4 4GB\", matches: [\"ddr44gb\"] },\r\n      { title: \"DDR4 8GB\", matches: [\"ddr48gb\"] },\r\n      { title: \"DDR4 16GB\", matches: [\"ddr416gb\"] },\r\n      { title: \"DDR4 32GB\", matches: [\"ddr432gb\"] },\r\n   ],\r\n   Size: [\r\n      { title: '11.5\"', matches: [\"11.5inches\", \"11.5inch\", '11.5\"', \"11.5\"] },\r\n      { title: '14\"', matches: [\"14inches\", \"14inch\", '14\"', \"14\"] },\r\n      { title: '15.6\"', matches: [\"15.6inches\", \"15.6inch\", , '15.6\"', \"15.6\"] },\r\n      { title: '16.1\"', matches: [\"16.1inches\", \"16.1inch\", , '16.1\"', \"16.1\"] },\r\n   ],\r\n   Resolution: [\r\n      { title: \"HD\", matches: [\"hd\"] },\r\n      { title: \"Full HD\", matches: [\"fhd\", \"fullhd\"] },\r\n      { title: \"4K\", matches: [\"4k\"] },\r\n   ],\r\n   Display: [\r\n      { title: \"HD\", matches: [\"hd\"] },\r\n      { title: \"Full HD\", matches: [\"fhd\", \"fullhd\"] },\r\n      { title: \"4K\", matches: [\"4k\"] },\r\n   ],\r\n   Storage: [\r\n      { title: \"250GB HDD\", matches: [\"2