n   ],\r\n};\r\n\r\nclass Key {\r\n   static None = \"none\";\r\n   static Available = \"isAvailable\";\r\n   static Brand = \"brand\";\r\n   static Size = \"size\";\r\n   static Ram = \"ram\";\r\n   static Storage = \"storage\";\r\n   static ResolutionDisplay = \"resolutiondisplay\";\r\n}\r\nclass Filter {\r\n   static Available = {\r\n      key: Key.Available,\r\n      title: \"Available\",\r\n\r\n      categoryAccessibles: Object.keys(CategoryKey).map((key) => CategoryKey[key]),\r\n\r\n      isCategoryKeyAccessible(key) {\r\n         return this.categoryAccessibles.includes(key);\r\n      },\r\n\r\n      toGroups(items) {\r\n         return [\r\n            {\r\n               title: \"Available\",\r\n               items: items.filter(\r\n                  (item) => !item.stock || (item.stock && item.stock.isAvailable)\r\n               ),\r\n            },\r\n            {\r\n               title: \"Unavailable\",\r\n               items: items.filter((item) => item.stock && !item.stock.isAvailable),\r\n            },\r\n         ];\r\n      },\r\n      sortGroups(groups) {\r\n         return groups.sort(this.arrangeGroups);\r\n      },\r\n      arrangeGroups(group1, group2) {\r\n         return 0;\r\n      },\r\n   };\r\n   static Brand = {\r\n      key: Key.Brand,\r\n      title: \"Brand\",\r\n\r\n      categoryAccessibles: Object.keys(CategoryKey).map((key) => CategoryKey[key]),\r\n\r\n      isCategoryKeyAccessible(key) {\r\n         return this.categoryAccessibles.includes(key);\r\n      },\r\n\r\n      toGroups(items) {\r\n         if (!Array.isArray(items)) return [];\r\n         return items.reduce((groups, item) => {\r\n            const { brand } = item;\r\n            return pushIntoGroups(\r\n               groups,\r\n               {\r\n                  key: brand ? brand.title : \"Other\",\r\n                  title: brand && !brand.icon ? brand.title : \"\",\r\n                  icon: brand && brand.icon ? brand.icon.fullPath : \"\",\r\n         