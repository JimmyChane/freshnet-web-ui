AAA,EAAA,OAAA,IAAA;AACA,aAAA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAAA,CAAA,GAAA,CAAA,qBAAA,CAFA;AAGA,QAAA,KAAA,EAAA;AAAA,iBAAA,KAAA,CAAA,KAAA,CAAA,kBAAA,CAAA;AAAA;AAHA,OAAA;AAKA;AAhBA,GANA;AAwBA,EAAA,KAAA,EAAA,CAAA,kBAAA;AAxBA,CAAA","sourcesContent":["<script>\r\n   import ActionbarProduct from \"@/pages/product/ActionBarProduct.vue\";\r\n   import ItemProduct from \"./ItemProduct.vue\";\r\n   import Empty from \"@/components/Empty.vue\";\r\n   import Footer from \"@/app/Footer.vue\";\r\n\r\n   export default {\r\n      components: { ActionbarProduct, ItemProduct, Empty, Footer },\r\n      props: { products: { type: Array, default: () => [] } },\r\n      data() {\r\n         return { scrollTop: 0 };\r\n      },\r\n      computed: {\r\n         isEditable() {\r\n            const { user } = this.loginStore.getters;\r\n            if (!user) return false;\r\n            return user.isTypeAdmin() || user.isTypeStaff();\r\n         },\r\n         categoryKey: (c) => c.$route.params.category,\r\n         productId: (c) => c.$route.query.productId,\r\n\r\n         initRightMenus() {\r\n            if (!this.isEditable) return null;\r\n            return {\r\n               title: \"Add\",\r\n               icon: this.host.res(\"icon/add-2A4858.svg\"),\r\n               click: () => this.$emit(\"click-productAdd\"),\r\n            };\r\n         },\r\n      },\r\n      emits: [\"click-productAdd\"],\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div class=\"PanelProducts\" @scroll=\"(event) => (scrollTop = event.target.scrollTop)\">\r\n      <ActionbarProduct\r\n         :class=\"[\r\n            'PanelProducts-actionbar',\r\n            scrollTop > 0 ? 'PanelProducts-actionbar-hasShadow' : '',\r\n         ]\"\r\n         :products=\"products\"\r\n         :rightMenus=\"initRightMenus\"\r\n      />\r\n\r\n      <div class=\"PanelProducts-body\">\r\n         <div class=\"PanelProducts-products\" v-if=\"products.length\">\r\n            <router-link\r\n               v-for=\"product of products\"\r\n               :key=\"product.getId()\"\r\n               :to=\"{ query: { productId: product.getId() } }\"\r\n            >\r\n               <ItemProduct :item=\"product\" :isSelected=\"productId === product._id\" />\r\n            </router-link>\r\n         </div>\r\n\r\n         <Empty v-if=\"!categoryKey && !products.length\" />\r\n      </div>\r\n\r\n      <Footer />\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .PanelProducts {\r\n      z-index: 1;\r\n      position: relative;\r\n      width: 100%;\r\n      height: 100%;\r\n      display: flex;\r\n      flex-direction: column;\r\n      justify-content: flex-start;\r\n      align-items: flex-start;\r\n      overflow-y: auto;\r\n\r\n      .PanelProducts-actionbar {\r\n         z-index: 2;\r\n         transition: var(--animation-duration);\r\n      }\r\n      .PanelProducts-actionbar-hasShadow {\r\n         box-shadow: 0 0 6rem hsla(0, 0%, 0%, 0.4);\r\n      }\r\n      .PanelProducts-body {\r\n         z-index: 1;\r\n         width: 100%;\r\n         flex-grow: 1;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: center;\r\n         justify-content: flex-start;\r\n\r\n         .PanelProducts-products {\r\n            width: 100%;\r\n            gap: 2.5rem 0.7rem;\r\n            padding: 1rem 1.5rem;\r\n            display: grid;\r\n            grid-template-columns: repeat(auto-fill, minmax(12rem, 1fr));\r\n\r\n            & > * {\r\n               text-decoration: none;\r\n               & > * {\r\n                  width: 100%;\r\n                  height: 100%;\r\n               }\r\n            }\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/product"}]}