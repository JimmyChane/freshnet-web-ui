ection: row;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      gap: 1rem;\r\n      position: sticky;\r\n      top: 0;\r\n      z-index: 3;\r\n      background: var(--actionbar-background-color);\r\n      padding: 1rem 1.2rem;\r\n      transition: var(--animatin-duration);\r\n      color: var(--primary-color);\r\n\r\n      @media (max-width: 800px) {\r\n         padding: 0.6rem 0.8rem;\r\n      }\r\n\r\n      .ActionBar2-leftMenus {\r\n         min-width: max-content;\r\n      }\r\n      .ActionBar2-rightMenus {\r\n         min-width: max-content;\r\n      }\r\n      .ActionBar2-title {\r\n         flex-grow: 0;\r\n         display: flex;\r\n         flex-direction: row;\r\n         align-items: center;\r\n         justify-content: flex-start;\r\n         font-size: 1.6rem;\r\n         font-weight: 400;\r\n         white-space: nowrap;\r\n         text-overflow: clip;\r\n         transition: var(--animatin-duration);\r\n         overflow: hidden;\r\n         color: inherit;\r\n         @media (max-width: 800px) {\r\n            font-size: 1.4rem;\r\n         }\r\n      }\r\n      .ActionBar2-main {\r\n         flex-grow: 1;\r\n         display: flex;\r\n         flex-direction: row;\r\n         align-items: center;\r\n         justify-content: flex-start;\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/components/navigation/actionbar2"}]}