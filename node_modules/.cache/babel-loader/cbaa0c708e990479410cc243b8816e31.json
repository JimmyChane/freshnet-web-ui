scription: {\r\n               isShowing: false,\r\n               customer: null,\r\n               device: null,\r\n            },\r\n         };\r\n      },\r\n      computed: {\r\n         isLoading: (c) => c.customerStore.getters.isLoading,\r\n         customers: (c) => c.customerStore.getters.customers,\r\n         currentCustomerId: (c) => c.$route.query.customerId,\r\n         currentCustomer: (c) => {\r\n            if (!c.customers.length) return null;\r\n            let customer = c.customers.find((customer) => {\r\n               return customer._id === c.currentCustomerId;\r\n            });\r\n            return customer ? customer : null;\r\n         },\r\n\r\n         isOver1200px: (c) => c.$root.window.innerWidth > 1200,\r\n\r\n         itemDrawerEdge: () => Drawer.Edge.RIGHT,\r\n         itemDrawerMode() {\r\n            if (!this.currentCustomer) return Drawer.Mode.DRAWER_COLLAPSE;\r\n            return this.isOver1200px ? Drawer.Mode.FIXED : Drawer.Mode.DRAWER_EXPAND;\r\n         },\r\n      },\r\n      watch: {\r\n         customers() {\r\n            this.invalidateCustomers();\r\n         },\r\n      },\r\n      mounted() {\r\n         this.invalidateCustomers();\r\n         this.customerStore.dispatch(\"getCustomers\");\r\n      },\r\n      methods: {\r\n         invalidateCustomers() {\r\n            /* if (!this.currentCustomer && this.customers.length) {\r\n               this.setCurrentCustomer(this.customers[0], false);\r\n            } */\r\n         },\r\n\r\n         setCurrentCustomer(item, isNext = true) {\r\n            this.$root.setRoute(\r\n               { query: { customerId: item ? item._id : null } },\r\n               isNext\r\n            );\r\n            this.bottomSheet.isShowing = isNext;\r\n         },\r\n\r\n         onStartAddCustomer() {\r\n            this.windowAddCustomer.isShowing = true;\r\n         },\r\n         onClickDismissAddCustomer() {\r\n            this.windowAddCustomer.isShowing = false;\r\n       