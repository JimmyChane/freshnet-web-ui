a = { title: \"\" };\r\n         },\r\n         clickConfirm() {\r\n            let { data } = this;\r\n            let output = {\r\n               title: data.title.trim(),\r\n               categoryId: this.categoryId,\r\n            };\r\n\r\n            if (!output.title) {\r\n               this.$root.feedback(\"You must specify title\");\r\n               return;\r\n            }\r\n\r\n            this.$emit(\"click-confirm\", output) && this.clear();\r\n         },\r\n      },\r\n      mounted() {\r\n         this.clear();\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <PopupWindowAction\r\n      class=\"BrowseWindowProductAdd\"\r\n      title=\"New Product\"\r\n      :isShowing=\"isShowing\"\r\n      @click-dismiss=\"$emit('click-dismiss') && clear()\"\r\n      @click-cancel=\"$emit('click-cancel') && clear()\"\r\n      @click-ok=\"clickConfirm()\"\r\n   >\r\n      <div class=\"BrowseWindowProductAdd-body\">\r\n         <div class=\"BrowseWindowProductAdd-category\" v-if=\"category\">\r\n            <span class=\"BrowseWindowProductAdd-reuse-title\">Category</span>\r\n            <div class=\"BrowseWindowProductAdd-category-body\">\r\n               <img\r\n                  class=\"BrowseWindowProductAdd-category-icon\"\r\n                  v-if=\"categoryIconUrl\"\r\n                  :src=\"categoryIconUrl\"\r\n               />\r\n               <span class=\"BrowseWindowProductAdd-category-title\">{{\r\n                  categoryTitle\r\n               }}</span>\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"BrowseWindowProductAdd-title\">\r\n            <span class=\"BrowseWindowProductAdd-reuse-title\">Title</span>\r\n            <input\r\n               class=\"BrowseWindowProductAdd-title-inputText\"\r\n               type=\"text\"\r\n               v-model=\"data.title\"\r\n            />\r\n            <span class=\"BrowseWindowProductAdd-reuse-required\">Required</span>\r\n         </div>\r\n      </div>\r\n   