\n            Requirement: CustomerModule.Requirement,\r\n            data: {},\r\n         };\r\n      },\r\n      computed: {\r\n         isLoading: (c) => c.customerStore.getters.isLoading,\r\n         isClickable: (c) => !c.customerStore.getters.isLoading,\r\n      },\r\n      watch: {\r\n         isShowing() {\r\n            if (!this.isShowing) return;\r\n            const { CustomerName } = this.$refs;\r\n            if (CustomerName) CustomerName.focus();\r\n         },\r\n      },\r\n      created() {\r\n         this.resetData();\r\n      },\r\n      mounted() {\r\n         this.resetData();\r\n      },\r\n      methods: {\r\n         resetData() {\r\n            this.resetDataOnDelay(0);\r\n         },\r\n         resetDataOnDelay(delay = 0) {\r\n            if (delay) {\r\n               setTimeout(() => this.resetDataOnDelay(0), delay);\r\n               return;\r\n            }\r\n\r\n            this.data = {\r\n               name: \"\",\r\n               phoneNumber: \"\",\r\n               description: \"\",\r\n            };\r\n         },\r\n\r\n         clickOk() {\r\n            this.data.name = this.data.name.trim();\r\n            this.data.phoneNumber = this.data.phoneNumber.trim();\r\n            this.data.description = this.data.description.trim();\r\n\r\n            if (this.Requirement.name.isRequired && !this.data.name) {\r\n               this.$root.feedback('You must specify the \"Name\"');\r\n            } else if (\r\n               this.Requirement.phoneNumber.isRequired &&\r\n               !this.data.phoneNumber\r\n            ) {\r\n               this.$root.feedback('You must specify the \"Phone Number\"');\r\n            } else if (\r\n               this.Requirement.description.isRequired &&\r\n               !this.data.description\r\n            ) {\r\n               this.$root.feedback('You must specify the \"Description\"');\r\n            } else {\r\n               this.customerStore.dispatch(\"addCustomer\", this.data).then((item) => {\r\n                  this.$emit(\"click-ok\", { item });\r\n                  this.resetDataOnDelay(700);\r\n               });\r\n            }\r\n         },\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <PopupWindowAction\r\n      class=\"WindowAddCustomer\"\r\n      title=\"Add New Customer\"\r\n      :isShowing=\"isShowing\"\r\n      :isLoading=\"isLoading\"\r\n      :isClickable=\"isClickable\"\r\n      @click-dismiss=\"$emit('click-dismiss')\"\r\n      @click-cancel=\"\r\n         () => {\r\n            $emit('click-cancel');\r\n            resetDataOnDelay(700);\r\n         }\r\n      \"\r\n      @click-ok=\"clickOk()\"\r\n   >\r\n      <div class=\"WindowAddCustomer-body\">\r\n         <WindowSection class=\"WindowAddCustomer-customer\">\r\n            <div class=\"WindowAddCustomer-customer-body\">\r\n               <Input\r\n                  class=\"WindowAddCustomer-reuse-input WindowAddCustomer-customer-part\"\r\n                  label=\"Name\"\r\n                  type=\"text\"\r\n                  :isRequired=\"true\"\r\n                  :bindValue=\"data.name\"\r\n                  @input=\"(comp) => (data.name = comp.value)\"\r\n               />\r\n               <Input\r\n                  class=\"WindowAddCustomer-reuse-input WindowAddCustomer-customer-part\"\r\n                  label=\"Phone Number\"\r\n                  type=\"text\"\r\n                  :bindValue=\"data.phoneNumber\"\r\n                  @input=\"(comp) => (data.phoneNumber = comp.value)\"\r\n               />\r\n            </div>\r\n         </WindowSection>\r\n\r\n         <WindowSection\r\n            class=\"WindowAddCustomer-description\"\r\n            title=\"Description\"\r\n            :isRequired=\"Requirement.description.isRequired\"\r\n         >\r\n            <textarea\r\n               class=\"WindowAddCustomer-reuse-input\"\r\n               type=\"text\"\r\n               v-model=\"data.description\"\r\n            />\r\n         </WindowSection>\r\n      </div>\r\n   </PopupWindowAction>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .WindowAddCustomer-reuse-input {\r\n      font-size: 1rem;\r\n      border: none;\r\n      background: hsla(0, 0%, 0%, 0.03);\r\n      border-bottom: 1px solid hsl(0, 0%, 70%);\r\n      border-radius: 0.2rem;\r\n      padding: 0.2rem 0.4rem;\r\n   }\r\n\r\n   .WindowAddCustomer {\r\n      width: 100%;\r\n      height: 100%;\r\n      .WindowAddCustomer-body {\r\n         width: 26rem;\r\n         max-width: 100%;\r\n         height: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         gap: 40px;\r\n\r\n         .WindowAddCustomer-customer {\r\n            width: 100%;\r\n            .WindowAddCustomer-customer-body {\r\n               width: 100%;\r\n               display: flex;\r\n               flex-direction: column;\r\n               gap: 1rem;\r\n               & > * {\r\n                  min-height: 2.5rem;\r\n               }\r\n            }\r\n         }\r\n         .WindowAddCustomer-description {\r\n            width: 100%;\r\n            .WindowAddCustomer-reuse-input {\r\n               height: 6rem;\r\n               resize: none;\r\n            }\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/customer/window"}]}