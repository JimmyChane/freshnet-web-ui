on/actionbar2/ActionBar2.vue\";\r\n   import LayoutProductViewer from \"@/pages/product/LayoutProductViewer/LayoutProductViewer.vue\";\r\n\r\n   export default {\r\n      components: { ActionBar2, LayoutProductViewer },\r\n      emits: [\r\n         \"click-dismiss\",\r\n         \"click-productRemove\",\r\n         \"click-product-imageRemove\",\r\n         \"click-product-titleBrandUpdate\",\r\n         \"click-product-priceUpdate\",\r\n         \"click-product-descriptionUpdate\",\r\n         \"click-product-categoryUpdate\",\r\n         \"click-product-specificationsUpdate\",\r\n      ],\r\n      props: {\r\n         product: { type: Object, default: () => null },\r\n         productPrevious: { type: Object, default: () => null },\r\n         productNext: { type: Object, default: () => null },\r\n\r\n         isWide: { type: Boolean, default: false },\r\n         isEditable: { type: Boolean, default: false },\r\n         isBackable: { type: Boolean, default: true },\r\n      },\r\n      computed: {\r\n         settings: (context) => {\r\n            let settings = context.settingStore.getters.items;\r\n            return Array.isArray(settings) ? settings : [];\r\n         },\r\n\r\n         actionbarLeftMenus() {\r\n            if (!this.isBackable) return [];\r\n            return [\r\n               {\r\n                  key: \"back\",\r\n                  title: \"Back\",\r\n                  icon: this.host.res(\"icon/arrow-left-000000.svg\"),\r\n                  click: () => this.$emit(\"click-dismiss\"),\r\n               },\r\n            ];\r\n         },\r\n         actionbarRightMenus() {\r\n            return [\r\n               this.product && this.isEditable\r\n                  ? {\r\n                       key: \"trash\",\r\n                       icon: this.host.res(\"icon/trash-000000.svg\"),\r\n                       click: () =>\r\n                          this.$emit(\"click-productRemove\", { product: this.product }),\r\n                    }\r\n                  : null,\r\n               this.product\r\n                  ? {\r\n                       key: \"view\",\r\n                       title: \"View\",\r\n                       icon: this.host.res(\"icon/view-000000.svg\"),\r\n                       click: () => this.clickView(),\r\n                    }\r\n                  : null,\r\n               this.product\r\n                  ? {\r\n                       key: \"copy\",\r\n                       title: \"Copy Link\",\r\n                       icon: this.host.res(\"icon/copy-000000.svg\"),\r\n                       click: () => this.clickCopyLink(),\r\n                    }\r\n                  : null,\r\n            ];\r\n         },\r\n\r\n         productLink: (context) => {\r\n            let { product } = context;\r\n            if (!product) return \"\";\r\n            return `${AppHost.path}/item/id/${product.getId()}`;\r\n         },\r\n\r\n         category: (context) => {\r\n            return context.categoryId\r\n               ? context.productStore.getters.categories.find((category) => {\r\n                    return category.getId() === context.categoryId;\r\n                 })\r\n               : null;\r\n         },\r\n         categoryId: (context) => {\r\n            return context.product ? context.product.getCategoryId() : \"\";\r\n         },\r\n         categoryTitle: (context) => {\r\n            return context.category ? context.category.getTitle() : \"\";\r\n         },\r\n\r\n         brand: (context) => {\r\n            return context.brandStore.getters.items.find((brand) => {\r\n               return brand.getId() === context.brandId;\r\n            });\r\n         },\r\n         brandId: (context) => (context.product ? context.product.getBrandId() : \"\"),\r\n         brandTitle: (context) => (context.brand ? context.brand.getTitle() : \"\"),\r\n\r\n         title: (context) => (context.product ? context.product.getTitle() : \"\"),\r\n\r\n         category: (context) => {\r\n            return context.categoryId\r\n               ? context.productStore.getters.categories.find(\r\n                    (category) => category.getId() === context.categoryId\r\n                 )\r\n               : null;\r\n         },\r\n         categoryId: (context) =>\r\n            context.product ? context.product.getCategoryId() : \"\",\r\n         categoryTitle: (context) =>\r\n            context.category ? context.category.getTitle() : \"\",\r\n\r\n         fullTitle: (context) => {\r\n            if (context.title && context.brandTitle)\r\n               return `${context.brandTitle} ${context.title}`;\r\n            if (context.title) return context.title;\r\n            if (context.brandTitle) return context.brandTitle;\r\n            return \"\";\r\n         },\r\n      },\r\n      watch: {\r\n         product() {\r\n            const { LayoutProductDetail } = this.$refs;\r\n            if (LayoutProductDetail) LayoutProductDetail.scrollTop = 0;\r\n         },\r\n      },\r\n      methods: {\r\n         clickCopyLink() {\r\n            if (!this.productLink) {\r\n               this.$root.feedback(\"Cannot Copy\");\r\n               return;\r\n            }\r\n            this.$root.copyText(this.productLink);\r\n            this.$root.feedback({\r\n               icon: this.host.res(\"icon/copy-FFFFFF.svg\"),\r\n               text: \"Copied to Clipboard\",\r\n               actions: [{ title: \"Open\", click: () => this.clickView() }],\r\n            });\r\n         },\r\n         clickView() {\r\n            let urlView = `${AppHost.path}/product/view`;\r\n\r\n            if (!this.product) {\r\n               this.$root.openLink(urlView);\r\n            } else {\r\n               this.$root.openLink(`${urlView}?productId=${this.product.getId()}`);\r\n            }\r\n         },\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div class=\"LayoutProductDetail\" ref=\"LayoutProductDetail\">\r\n      <ActionBar2\r\n         class=\"LayoutProductDetail-actionbar\"\r\n         :leftMenus=\"actionbarLeftMenus\"\r\n         :rightMenus=\"actionbarRightMenus\"\r\n      >\r\n         <div\r\n            class=\"LayoutProductDetail-actionbar-body\"\r\n            v-if=\"categoryTitle || fullTitle\"\r\n         >\r\n            <span class=\"LayoutProductDetail-actionbar-category\" v-if=\"categoryTitle\">{{\r\n               categoryTitle\r\n            }}</span>\r\n            <span class=\"LayoutProductDetail-actionbar-title\" v-if=\"fullTitle\">{{\r\n               fullTitle\r\n            }}</span>\r\n         </div>\r\n      </ActionBar2>\r\n\r\n      <LayoutProductViewer\r\n         class=\"LayoutProductDetail-main\"\r\n         :product=\"product\"\r\n         :productPrevious=\"productPrevious\"\r\n         :productNext=\"productNext\"\r\n         :isWide=\"isWide\"\r\n         :isEditable=\"isEditable\"\r\n         @click-product-imageRemove=\"(x) => $emit('click-product-imageRemove', x)\"\r\n         @click-product-titleBrandUpdate=\"\r\n            (x) => $emit('click-product-titleBrandUpdate', x)\r\n         \"\r\n         @click-product-priceUpdate=\"(x) => $emit('click-product-priceUpdate', x)\"\r\n         @click-product-descriptionUpdate=\"\r\n            (x) => $emit('click-product-descriptionUpdate', x)\r\n         \"\r\n         @click-product-categoryUpdate=\"(x) => $emit('click-product-categoryUpdate', x)\"\r\n         @click-product-specificationsUpdate=\"\r\n            (x) => $emit('click-product-specificationsUpdate', x)\r\n         \"\r\n      />\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .LayoutProductDetail {\r\n      width: 100%;\r\n      height: 100%;\r\n\r\n      background: white;\r\n      font-size: 1.2rem;\r\n\r\n      overflow-y: auto;\r\n      position: relative;\r\n\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      flex-direction: column;\r\n      align-items: center;\r\n\r\n      .LayoutProductDetail-actionbar {\r\n         z-index: 3;\r\n         width: 100%;\r\n         box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.15);\r\n         .LayoutProductDetail-actionbar-body {\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: flex-start;\r\n            justify-content: center;\r\n            gap: 0.1rem;\r\n            line-height: 1.1rem;\r\n            .LayoutProductDetail-actionbar-category {\r\n               font-weight: 900;\r\n               font-size: 1.4rem;\r\n               line-height: 1rem;\r\n               max-height: 2.2rem;\r\n               overflow: hidden;\r\n               color: black;\r\n            }\r\n            .LayoutProductDetail-actionbar-title {\r\n               font-size: 1.2rem;\r\n               line-height: 1rem;\r\n               max-height: 2.2rem;\r\n               overflow: hidden;\r\n               color: black;\r\n            }\r\n         }\r\n      }\r\n\r\n      .LayoutProductDetail-main {\r\n         z-index: 2;\r\n         flex-grow: 1;\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/product"}]}