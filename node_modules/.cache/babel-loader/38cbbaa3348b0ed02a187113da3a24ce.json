,eAAA,QAAA,CAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA,UAAA;AACA,OAFA,CAAA;AAGA,UAAA,QAAA,EAAA,OAAA,QAAA;AAEA,aAAA,KAAA,UAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,GAAA,KAAA,cAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA,KAXA;AAYA,IAAA,UAAA,EAAA,oBAAA,CAAA;AAAA,aAAA,CAAA,CAAA,QAAA,GAAA,CAAA,CAAA,QAAA,CAAA,GAAA,GAAA,EAAA;AAAA,KAZA;AAcA,IAAA,MAdA,oBAcA;AACA,aAAA,KAAA,UAAA,CAAA,OAAA,CAAA,KAAA;AACA;AAhBA,GAbA;AA+BA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,KAAA;AACA;AAHA,GA/BA;AAoCA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,IAAA,GAAA;AAAA,QAAA,KAAA,EAAA,EAAA;AAAA,QAAA,OAAA,EAAA,EAAA;AAAA,QAAA,UAAA,EAAA;AAAA,OAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,KAJA;AAKA,IAAA,YALA,0BAKA;AACA,uBAAA,KAAA,IAAA;AAAA,UAAA,KAAA,cAAA,KAAA;AAAA,UAAA,OAAA,cAAA,OAAA;AAAA,UAAA,UAAA,cAAA,UAAA;AACA,UAAA,MAAA,GAAA;AAAA,QAAA,KAAA,EAAA,KAAA,CAAA,IAAA,EAAA;AAAA,QAAA,OAAA,EAAA,OAAA;AAAA,QAAA,UAAA,EAAA;AAAA,OAAA;;AAEA,UAAA,CAAA,MAAA,CAAA,KAAA,EAAA;AACA,aAAA,UAAA,GAAA,eAAA;AACA,aAAA,KAAA,CAAA,QAAA,CAAA,eAAA;AACA;AACA;;AAEA,WAAA,KAAA,CAAA,eAAA,EAAA,MAAA,KAAA,KAAA,KAAA,EAAA;AACA;AAhBA,GApCA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,SAAA,KAAA;AACA,SAAA,UAAA,CAAA,QAAA,CAAA,UAAA;AACA,SAAA,aAAA,CAAA,QAAA,CAAA,UAAA;AACA,SAAA,YAAA,CAAA,QAAA,CAAA,aAAA;AACA;AA3DA,CAAA","sourcesContent":["<script>\r\n   import ModuleCategory from \"@/../../express/data_structures/Category.js\";\r\n   import PopupWindowAction from \"@/components/window/PopupWindowAction.vue\";\r\n   import Input from \"@/components/Input.vue\";\r\n   import Selector3 from \"@/components/selector/Selector3.vue\";\r\n\r\n   export default {\r\n      components: { PopupWindowAction, Input, Selector3 },\r\n      emits: [\"callback-cancel\", \"callback-confirm\"],\r\n      props: {\r\n         isShowing: { type: Boolean, default: false },\r\n         input: { type: Object, default: () => null },\r\n      },\r\n      data() {\r\n         return {\r\n            data: { title: \"\", brandId: \"\", categoryId: \"\" },\r\n            titleError: \"\",\r\n         };\r\n      },\r\n      computed: {\r\n         categories: (c) => c.categoryStore.getters.items,\r\n         category() {\r\n            const category = this.categories.find((category) => {\r\n               return category._id === this.input.categoryId;\r\n            });\r\n            if (category) return category;\r\n\r\n            return this.categories.find((category) => {\r\n               return category.key === ModuleCategory.Key.Other;\r\n            });\r\n         },\r\n         categoryId: (c) => (c.cateogry ? c.category._id : \"\"),\r\n\r\n         brands() {\r\n            return this.brandStore.getters.items;\r\n         },\r\n      },\r\n      watch: {\r\n         input() {\r\n            this.clear();\r\n         },\r\n      },\r\n      methods: {\r\n         clear() {\r\n            this.data = { title: \"\", brandId: \"\", categoryId: \"\" };\r\n            this.titleError = \"\";\r\n         },\r\n         clickConfirm() {\r\n            let { title, brandId, categoryId } = this.data;\r\n            let output = { title: title.trim(), brandId, categoryId };\r\n\r\n            if (!output.title) {\r\n               this.titleError = \"Title Missing\";\r\n               this.$root.feedback(\"Title Missing\");\r\n               return;\r\n            }\r\n\r\n            this.$emit(\"click-confirm\", output) && this.clear();\r\n         },\r\n      },\r\n      mounted() {\r\n         this.clear();\r\n         this.brandStore.dispatch(\"getItems\");\r\n         this.categoryStore.dispatch(\"getItems\");\r\n         this.productStore.dispatch(\"getProducts\");\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <PopupWindowAction\r\n      class=\"WindowAddProduct\"\r\n      title=\"New Product\"\r\n      :isShowing=\"isShowing\"\r\n      @click-dismiss=\"$emit('click-dismiss') && clear()\"\r\n      @click-cancel=\"$emit('click-cancel') && clear()\"\r\n      @click-ok=\"clickConfirm()\"\r\n   >\r\n      <div class=\"WindowAddProduct-body\">\r\n         <Input\r\n            class=\"WindowAddProduct-title\"\r\n            label=\"Title\"\r\n            :isRequired=\"true\"\r\n            :bindValue=\"data.title\"\r\n            :error=\"titleError\"\r\n            @input=\"(comp) => (data.title = comp.value)\"\r\n         />\r\n\r\n         <Selector3\r\n            :menus=\"[\r\n               { key: 'none', title: 'None' },\r\n               ...brands.map((brand) => {\r\n                  return {\r\n                     key: brand._id,\r\n                     title: brand.title,\r\n                     icon: brand.icon ? brand.icon.url({ height: 10 }) : '',\r\n                  };\r\n               }),\r\n            ]\"\r\n            defaultKey=\"\"\r\n            :selectedKey=\"data.brandId\"\r\n            @click-menu=\"(menu) => (data.brandId = menu.key)\"\r\n         />\r\n\r\n         <Selector3\r\n            :menus=\"\r\n               categories.map((category) => {\r\n                  return {\r\n                     key: category._id,\r\n                     title: category.title,\r\n                     icon: category.icon ? category.icon.url({ height: 10 }) : '',\r\n                  };\r\n               })\r\n            \"\r\n            :selectedKey=\"data.categoryId\"\r\n            @click-menu=\"(menu) => (data.categoryId = menu.key)\"\r\n         />\r\n      </div>\r\n   </PopupWindowAction>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .WindowAddProduct-reuse-title {\r\n      font-size: 1.1rem;\r\n      font-weight: 900;\r\n   }\r\n   .WindowAddProduct-reuse-required {\r\n      color: hsl(0, 50%, 50%);\r\n      font-size: 0.8rem;\r\n   }\r\n\r\n   .WindowAddProduct {\r\n      .WindowAddProduct-body {\r\n         width: 26rem;\r\n         max-width: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         gap: 3em;\r\n         .WindowAddProduct-category {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n            .WindowAddProduct-category-body {\r\n               display: flex;\r\n               flex-direction: row;\r\n               flex-wrap: nowrap;\r\n               align-items: center;\r\n               gap: 0.5rem;\r\n            }\r\n            .WindowAddProduct-category-icon {\r\n               width: 1.4rem;\r\n               height: 1.4rem;\r\n            }\r\n            .WindowAddProduct-category-title {\r\n               font-size: 1.2rem;\r\n            }\r\n         }\r\n         .WindowAddProduct-title {\r\n            padding-left: 0;\r\n            padding-right: 0;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/pages/product"}]}