<script>\r\n   import ButtonIcon from \"@/components/button/ButtonIcon.vue\";\r\n   export default {\r\n      components: { ButtonIcon },\r\n      props: {\r\n         item: { type: Object, default: () => null },\r\n         isSelected: { type: Boolean, default: false },\r\n         isSelectedDark: { type: Boolean, default: true },\r\n         isWide: { type: Boolean, default: true },\r\n      },\r\n      computed: {\r\n         itemIcon() {\r\n            if (!this.item || !this.item.icon) return null;\r\n            return this.item.icon;\r\n         },\r\n         icon() {\r\n            if (!this.itemIcon || !this.itemIcon.light) return \"\";\r\n            if (!this.isSelected) return this.itemIcon.dark;\r\n            if (this.isSelectedDark) return this.itemIcon.light;\r\n            return this.itemIcon.dark;\r\n         },\r\n         title: (context) => context.item.title,\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      :class=\"[\r\n         'LeftNavigationItem',\r\n         `LeftNavigationItem-${isWide ? 'isWide' : 'isThin'}`,\r\n      ]\"\r\n   >\r\n      <img class=\"LeftNavigationItem-icon\" v-if=\"icon\" :src=\"icon\" />\r\n      <span class=\"LeftNavigationItem-title\" v-if=\"isWide && title\">{{ title }}</span>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .LeftNavigationItem {\r\n      border: none;\r\n      text-align: center;\r\n      font-size: 1em;\r\n      background: none;\r\n      display: flex;\r\n      flex-direction: row;\r\n      gap: 1em;\r\n\r\n      .LeftNavigationItem-icon {\r\n         --size: 1.2em;\r\n         width: var(--size);\r\n         height: var(--size);\r\n         min-width: var(--size);\r\n         min-height: var(--size);\r\n         max-width: var(--size);\r\n         max-height: var(--size);\r\n         object-fit: scale-down;\r\n      }\r\n      .LeftNavigationItem-title {\r\n         flex-grow: 1;\r\n         min-height: 1.2em;\r\n         text-align: start;\r\n         font-size: 1em;\r\n         font-weight: 900;\r\n         line-height: 1.2;\r\n         color: inherit;\r\n\r\n         display: flex;\r\n         flex-direction: row;\r\n         align-items: center;\r\n      }\r\n   }\r\n\r\n   .LeftNavigationItem-isWide {\r\n      width: 100%;\r\n      justify-content: space-between;\r\n   }\r\n   .LeftNavigationItem-isThin {\r\n      align-items: center;\r\n      justify-content: center;\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/app"}]}