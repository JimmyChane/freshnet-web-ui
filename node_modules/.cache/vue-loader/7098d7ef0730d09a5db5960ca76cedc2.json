rs.categories.find((category) => {\r\n               return category.getId() === c.categoryId;\r\n            }),\r\n         categoryTitle: (c) => (c.category ? c.category.title : \"\"),\r\n         categoryIcon: (c) => (c.category ? c.category.getIcon() : null),\r\n         categoryIconUrl: (c) => (c.categoryIcon ? c.categoryIcon.url() : \"\"),\r\n      },\r\n      watch: {\r\n         input() {\r\n            this.clear();\r\n         },\r\n      },\r\n      methods: {\r\n         clear() {\r\n            this.data = { title: \"\" };\r\n         },\r\n         clickConfirm() {\r\n            let { data } = this;\r\n            let output = {\r\n               title: data.title.trim(),\r\n               categoryId: this.categoryId,\r\n            };\r\n\r\n            if (!output.title) {\r\n               this.$root.feedback(\"You must specify title\");\r\n               return;\r\n            }\r\n\r\n            this.$emit(\"click-confirm\", output) && this.clear();\r\n         },\r\n      },\r\n      mounted() {\r\n         this.clear();\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <PopupWindowAction\r\n      class=\"BrowseWindowProductAdd\"\r\n      title=\"New Product\"\r\n      :isShowing=\"isShowing\"\r\n      @click-dismiss=\"$emit('click-dismiss') && clear()\"\r\n      @click-cancel=\"$emit('click-cancel') && clear()\"\r\n      @click-ok=\"clickConfirm()\"\r\n   >\r\n      <div class=\"BrowseWindowProductAdd-body\">\r\n         <div class=\"BrowseWindowProductAdd-category\" v-if=\"category\">\r\n            <span class=\"BrowseWindowProductAdd-reuse-title\">Category</span>\r\n            <div class=\"BrowseWindowProductAdd-category-body\">\r\n               <img\r\n                  class=\"BrowseWindowProductAdd-category-icon\"\r\n                  v-if=\"categoryIconUrl\"\r\n                  :src=\"categoryIconUrl\"\r\n               />\r\n               <span class=\"BrowseWindowProductAdd-category-title\">{{\r\n                  categoryTitle\r\n               }}</span>\r\n            </div>\r\n         </div>\r\n\r\n         <Input\r\n            class=\"BrowseWindowProductAdd-title\"\r\n            label=\"Title\"\r\n            :isRequired=\"true\"\r\n            :bindValue=\"data.title\"\r\n            @input=\"(comp) => (data.title = comp.value)\"\r\n         />\r\n      </div>\r\n   </PopupWindowAction>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .BrowseWindowProductAdd-reuse-title {\r\n      font-size: 1.1rem;\r\n      font-weight: 900;\r\n   }\r\n   .BrowseWindowProductAdd-reuse-required {\r\n      color: hsl(0, 50%, 50%);\r\n      font-size: 0.8rem;\r\n   }\r\n\r\n   .BrowseWindowProductAdd {\r\n      .BrowseWindowProductAdd-body {\r\n         width: 26rem;\r\n         max-width: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: stretch;\r\n         gap: 3em;\r\n         .BrowseWindowProductAdd-category {\r\n            display: flex;\r\n            flex-direction: column;\r\n            gap: 10px;\r\n            .BrowseWindowProductAdd-category-body {\r\n               display: flex;\r\n               flex-direction: row;\r\n               flex-wrap: nowrap;\r\n               align-items: center;\r\n               gap: 0.5rem;\r\n            }\r\n            .BrowseWindowProductAdd-category-icon {\r\n               width: 1.4rem;\r\n               height: 1.4rem;\r\n            }\r\n            .BrowseWindowProductAdd-category-title {\r\n               font-size: 1.2rem;\r\n            }\r\n         }\r\n         .BrowseWindowProductAdd-title {\r\n            padding-left: 0;\r\n            padding-right: 0;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"]}]}