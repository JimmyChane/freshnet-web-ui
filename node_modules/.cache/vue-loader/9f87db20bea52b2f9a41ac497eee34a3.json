{"remainingRequest":"C:\\dev\\freshnet-v2\\vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\dev\\freshnet-v2\\vue\\src\\pages\\order\\PageOrder.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\dev\\freshnet-v2\\vue\\src\\pages\\order\\PageOrder.vue","mtime":1664551315825},{"path":"C:\\dev\\freshnet-v2\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1662650916242},{"path":"C:\\dev\\freshnet-v2\\vue\\node_modules\\babel-loader\\lib\\index.js","mtime":1662650915901},{"path":"C:\\dev\\freshnet-v2\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1662650916242},{"path":"C:\\dev\\freshnet-v2\\vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1662650934771}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["PageOrder.vue"],"names":[],"mappings":";AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"PageOrder.vue","sourceRoot":"src/pages/order","sourcesContent":["<script>\r\n   import PopupWindow from \"@/components/window/PopupWindow.vue\";\r\n   import Loadingv1 from \"@/components/Loading\";\r\n   import Empty from \"@/components/Empty.vue\";\r\n   import ActionBarManage from \"@/pages/manage/ActionBarManage.vue\";\r\n   import SectionOrder from \"./SectionOrder.vue\";\r\n   import Input from \"@/components/Input.vue\";\r\n\r\n   export default {\r\n      key: \"order\",\r\n      name: \"ViewOrder\",\r\n      title: \"Order\",\r\n      icon: { light: \"order-FFFFFF\", dark: \"order-2A4858\" },\r\n      userPermissions: [\"admin\", \"staff\"],\r\n\r\n      components: {\r\n         PopupWindow,\r\n         Loadingv1,\r\n         Empty,\r\n         ActionBarManage,\r\n         SectionOrder,\r\n         Input,\r\n      },\r\n      emits: [\"callback-side-expand\"],\r\n      data() {\r\n         return {\r\n            display: {\r\n               showDialogAppendOrder: false,\r\n               currentExpandedOrderid: \"\",\r\n            },\r\n\r\n            contentError: \"\",\r\n\r\n            customer_name: \"\",\r\n            phone_number: \"\",\r\n            content: \"\",\r\n\r\n            scrollTop: 0,\r\n         };\r\n      },\r\n      computed: {\r\n         isLoading: (c) => c.orderStore.getters.isLoading,\r\n         orders: (c) => {\r\n            let items = c.orderStore.getters.items;\r\n            return Array.isArray(items) ? items : [];\r\n         },\r\n         orderPendings: (c) => c.orderStore.getters.pendingGroupItems,\r\n         orderCompleteds: (c) => c.orderStore.getters.completedGroupItems,\r\n      },\r\n      mounted() {\r\n         this.orderStore.dispatch(\"refresh\").catch((error) => {\r\n            this.$root.feedback(\"Error While Loading Order\");\r\n            console.error(error);\r\n         });\r\n      },\r\n      methods: {\r\n         refresh() {\r\n            this.orderStore.dispatch(\"refresh\").catch((error) => {\r\n               this.$root.feedback(\"Error While Refreshing Order\");\r\n               console.error(error);\r\n            });\r\n         },\r\n         createOrder() {\r\n            const { customer_name, phone_number, content } = this;\r\n\r\n            if (content.trim() === \"\") {\r\n               this.contentError = \"Missing Content\";\r\n               return;\r\n            }\r\n\r\n            const order = {\r\n               customer_name: customer_name.trim(),\r\n               phone_number: phone_number.trim(),\r\n               content: content.trim(),\r\n            };\r\n\r\n            if (order.customer_name === \"\") delete order.customer_name;\r\n            if (order.phone_number === \"\") delete order.phone_number;\r\n\r\n            this.orderStore\r\n               .dispatch(\"addItem\", { data: order })\r\n               .then((order) => {\r\n                  this.inputCustomerName = \"\";\r\n                  this.phone_number = \"\";\r\n                  this.content = \"\";\r\n                  this.display.showDialogAppendOrder = false;\r\n               })\r\n               .catch((error) => {\r\n                  this.$root.feedback(\"Error While Creating Order\");\r\n               });\r\n         },\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      class=\"PageOrder\"\r\n      ref=\"orderRoot\"\r\n      @scroll=\"(event) => (scrollTop = event.target.scrollTop)\"\r\n   >\r\n      <ActionBarManage\r\n         class=\"PageOrder-actionbar\"\r\n         :hasShadow=\"scrollTop > 0\"\r\n         :title=\"$options.title\"\r\n         :rightMenus=\"[\r\n            {\r\n               key: 'appendOrder',\r\n               title: 'Append Order',\r\n               icon: host.res('icon/add-2A4858.svg'),\r\n               click: () =>\r\n                  (display.showDialogAppendOrder = !display.showDialogAppendOrder),\r\n            },\r\n            {\r\n               key: 'refresh',\r\n               title: 'Refresh',\r\n               icon: host.res('icon/refresh-2A4858.svg'),\r\n               click: refresh,\r\n            },\r\n         ]\"\r\n         @click-drawer-expand=\"$emit('click-drawer-expand')\"\r\n      />\r\n\r\n      <main v-if=\"orders.length\">\r\n         <SectionOrder\r\n            class=\"main-section\"\r\n            title=\"Pending\"\r\n            :items=\"orderPendings\"\r\n            :currentItemIdSelected=\"display.currentExpandedOrderid\"\r\n            @click-collapse=\"(item) => (display.currentExpandedOrderid = '')\"\r\n            @click-expand=\"(item) => (display.currentExpandedOrderid = item.id)\"\r\n            @click-complete=\"\r\n               (item) =>\r\n                  orderStore.dispatch('updateToCompletedOfId', {\r\n                     id: item.id,\r\n                  })\r\n            \"\r\n            @click-remove=\"\r\n               (item) => orderStore.dispatch('removeOItemOfId', { id: item.id })\r\n            \"\r\n         />\r\n\r\n         <SectionOrder\r\n            class=\"main-section\"\r\n            title=\"Completed\"\r\n            :items=\"orderCompleteds\"\r\n            :currentItemIdSelected=\"display.currentExpandedOrderid\"\r\n            @click-collapse=\"(item) => (display.currentExpandedOrderid = '')\"\r\n            @click-expand=\"(item) => (display.currentExpandedOrderid = item.id)\"\r\n            @click-pending=\"\r\n               (item) =>\r\n                  orderStore.dispatch('updateToPendingOfId', {\r\n                     id: item.id,\r\n                  })\r\n            \"\r\n            @click-remove=\"\r\n               (item) => orderStore.dispatch('removeOItemOfId', { id: item.id })\r\n            \"\r\n         />\r\n      </main>\r\n\r\n      <Empty v-if=\"!orders.length && !isLoading\" />\r\n\r\n      <Loadingv1 class=\"viewOrder-loading\" :isRunning=\"orderStore.getters.isLoading\" />\r\n\r\n      <PopupWindow\r\n         :style=\"{ 'z-index': '20' }\"\r\n         :isShowing=\"display.showDialogAppendOrder\"\r\n         @click-dismiss=\"display.showDialogAppendOrder = false\"\r\n      >\r\n         <div class=\"pageOrder-dialog-newOrder-body\">\r\n            <h1>New Order</h1>\r\n\r\n            <Input\r\n               class=\"PageOrder-input\"\r\n               label=\"Content\"\r\n               :isRequired=\"true\"\r\n               :error=\"contentError\"\r\n               :bindValue=\"content\"\r\n               @input=\"(comp) => (content = comp.value)\"\r\n            />\r\n            <Input\r\n               class=\"PageOrder-input\"\r\n               label=\"Customer Name\"\r\n               :bindValue=\"customer_name\"\r\n               @input=\"(comp) => (customer_name = comp.value)\"\r\n            />\r\n            <Input\r\n               class=\"PageOrder-input\"\r\n               label=\"Customer Phone Number\"\r\n               :bindValue=\"phone_number\"\r\n               @input=\"(comp) => (phone_number = comp.value)\"\r\n            />\r\n\r\n            <div class=\"buttons\">\r\n               <div class=\"body\">\r\n                  <button\r\n                     class=\"button-focus-none\"\r\n                     @click=\"display.showDialogAppendOrder = false\"\r\n                  >\r\n                     Cancel\r\n                  </button>\r\n               </div>\r\n\r\n               <div class=\"body\">\r\n                  <button class=\"button-focus\" @click=\"createOrder\">Submit</button>\r\n               </div>\r\n            </div>\r\n         </div>\r\n      </PopupWindow>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .PageOrder {\r\n      --width-max: 550px;\r\n\r\n      position: relative;\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      align-items: stretch;\r\n\r\n      overflow-y: auto;\r\n      height: 100%;\r\n      width: 100%;\r\n      padding-bottom: 80px;\r\n      transition: var(--animation-duration);\r\n\r\n      main {\r\n         width: 100%;\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: center;\r\n         padding-top: 10px;\r\n\r\n         .main-section {\r\n            margin-top: 10px;\r\n            margin-bottom: 20px;\r\n            background-color: white;\r\n            box-shadow: 0 2px 4px #b5b5b5;\r\n            padding: 10px;\r\n            transition: var(--animation-duration);\r\n         }\r\n         @media (min-width: 550px) {\r\n            padding-left: 20px;\r\n            padding-right: 20px;\r\n            .main-section {\r\n               border-radius: 6px;\r\n            }\r\n         }\r\n      }\r\n\r\n      .viewOrder-item {\r\n         .viewOrder-item-seperator {\r\n            margin: 4px 0;\r\n            min-width: auto;\r\n            min-height: 0.5px;\r\n            background-color: hsl(0, 0%, 84%);\r\n            background: hsla(0, 0%, 0%, 0.1);\r\n         }\r\n      }\r\n\r\n      .viewOrder-loading {\r\n         position: absolute;\r\n      }\r\n   }\r\n\r\n   .pageOrder-dialog-newOrder-body {\r\n      --color-shadow: #000000e5;\r\n      --max-width: 500px;\r\n      --padding-horizontal: 30px;\r\n      --padding-vertical: 40px;\r\n      --gap-all: 10px;\r\n\r\n      width: 100vw;\r\n      max-width: 500px;\r\n      height: max-content;\r\n      align-self: center;\r\n      display: flex;\r\n      flex-direction: column;\r\n      flex-wrap: nowrap;\r\n      background-color: white;\r\n      border-radius: 10px;\r\n      padding: 80px 60px;\r\n      overflow-x: hidden;\r\n      overflow-y: auto;\r\n      cursor: initial;\r\n      pointer-events: unset;\r\n\r\n      .PageOrder-input {\r\n         margin-top: 2rem;\r\n      }\r\n\r\n      h1 {\r\n         margin-bottom: 10px;\r\n      }\r\n\r\n      .buttons {\r\n         display: flex;\r\n         flex-direction: row;\r\n         column-gap: 30px;\r\n         margin-top: 40px;\r\n\r\n         .body {\r\n            width: calc(100% - 20px);\r\n            max-width: calc(var(--width-max) - 20px);\r\n\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-basis: auto;\r\n\r\n            flex-grow: 1;\r\n\r\n            button {\r\n               flex-grow: 1;\r\n\r\n               border: none;\r\n               border-radius: 10px;\r\n\r\n               padding: 10px 20px;\r\n               color: white;\r\n               font-weight: 600;\r\n               background-color: var(--accent-color);\r\n               transition: all var(--animation-duration) linear;\r\n\r\n               cursor: pointer;\r\n\r\n               outline: none;\r\n\r\n               &:hover,\r\n               &:focus {\r\n                  box-shadow: 0 0 8px #828282;\r\n               }\r\n            }\r\n\r\n            .button-focus-none {\r\n               border: 2px solid var(--accent-color);\r\n               background-color: white;\r\n               color: var(--accent-color);\r\n\r\n               &:hover,\r\n               &:focus {\r\n                  background-color: var(--accent-color);\r\n                  color: white;\r\n               }\r\n            }\r\n\r\n            .button-focus {\r\n               border: 2px solid var(--accent-color);\r\n               background-color: var(--accent-color);\r\n               color: white;\r\n\r\n               &:hover,\r\n               &:focus {\r\n                  border: 2px solid var(--accent-color);\r\n                  background-color: var(--accent-color);\r\n               }\r\n            }\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"]}]}