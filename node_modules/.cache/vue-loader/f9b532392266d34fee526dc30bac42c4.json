  </div>\r\n            <span class=\"ItemEvent-description\">{{ description }}</span>\r\n         </div>\r\n      </div>\r\n\r\n      <div class=\"ItemEvent-right\">\r\n         <span class=\"ItemEvent-result\" v-if=\"methodResult\">{{ methodResult }}</span>\r\n\r\n         <MenuButton\r\n            class=\"ItemEvent-menu\"\r\n            :menus=\"[\r\n               {\r\n                  key: 'delete',\r\n                  title: 'Delete',\r\n                  icon: host.res('icon/page/service/rejected-color.svg'),\r\n                  interact: () => $emit('callback-delete', item),\r\n               },\r\n            ]\"\r\n         />\r\n      </div>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .ItemEvent {\r\n      --border-radius: 0.4rem;\r\n\r\n      display: flex;\r\n      flex-direction: row;\r\n      flex-wrap: nowrap;\r\n      border-radius: var(--border-radius);\r\n      background: white;\r\n\r\n      .ItemEvent-left {\r\n         display: flex;\r\n         flex-direction: row;\r\n         flex-wrap: nowrap;\r\n         flex-grow: 1;\r\n         border-radius: var(--border-radius) 0 0 var(--border-radius);\r\n         overflow: hidden;\r\n         .ItemEvent-label {\r\n            background-color: var(--primary-color);\r\n            height: 100%;\r\n            width: 0.2rem;\r\n         }\r\n         .ItemEvent-middle {\r\n            padding: 0.6rem;\r\n            gap: 0.2rem;\r\n            display: flex;\r\n            flex-direction: column;\r\n            flex-grow: 1;\r\n            justify-content: space-between;\r\n\r\n            .ItemEvent-header {\r\n               display: flex;\r\n               flex-direction: row;\r\n               align-items: center;\r\n               justify-content: flex-start;\r\n               gap: 0.5rem;\r\n               font-size: 0.7rem;\r\n               & > * {\r\n                  min-width: max-content;\r\n                  width: max-content;\r\n               }\r\n               .ItemService-header-dot {\r\n                  width: 0.2rem;\r\n                  height: 0.2rem;\r\n                  background: rgb(0, 0, 0);\r\n                  border-radius: 50%;\r\n               }\r\n            }\r\n\r\n            .ItemEvent-description {\r\n               width: 100%;\r\n               line-height: 1.1;\r\n               font-size: 0.9rem;\r\n               white-space: pre-line;\r\n            }\r\n         }\r\n      }\r\n      .ItemEvent-right {\r\n         display: flex;\r\n         flex-direction: row;\r\n         align-items: center;\r\n         flex-wrap: nowrap;\r\n         padding: 0.8rem;\r\n         gap: 0.8rem;\r\n         border-radius: 0 var(--border-radius) var(--border-radius) 0;\r\n\r\n         .ItemEvent-result {\r\n            z-index: 2;\r\n            height: 100%;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: center;\r\n            margin-right: -3.3rem;\r\n\r\n            background: var(--primary-color);\r\n            color: white;\r\n            text-align: center;\r\n            font-size: 0.8rem;\r\n            padding: 0.2rem 0.4rem;\r\n            border-radius: 0.3rem;\r\n            transition: var(--animation-duration);\r\n         }\r\n         .ItemEvent-menu {\r\n            z-index: 1;\r\n            opacity: 0;\r\n         }\r\n      }\r\n\r\n      &:hover {\r\n         .ItemEvent-result {\r\n            z-index: 1;\r\n            margin-right: 0;\r\n         }\r\n         .ItemEvent-menu {\r\n            z-index: 2;\r\n            opacity: 1;\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"]}]}