edback-input-body\">\r\n                                 <textarea\r\n                                    class=\"Home-feedback-input\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Write something...\"\r\n                                 />\r\n                              </div>\r\n                           </div>\r\n                        </Section> -->\r\n               </div>\r\n\r\n               <Footer />\r\n            </div>\r\n         </div>\r\n      </div>\r\n\r\n      <ItemFloatingContactLink\r\n         class=\"PageHome-floating-telephone2\"\r\n         :class=\"[isOver900px ? 'PageHome-floating-telephone2-isScreenWide' : '']\"\r\n         :isScreenWide=\"isOver1200px\"\r\n         href=\"tel:+60332811526\"\r\n         :src=\"host.res('icon/telephone-color.svg')\"\r\n         title1=\"Call 03-3281-1526\"\r\n         title2=\"Office\"\r\n      />\r\n      <ItemFloatingContactLink\r\n         class=\"PageHome-floating-telephone1\"\r\n         :class=\"[isOver900px ? 'PageHome-floating-telephone1-isScreenWide' : '']\"\r\n         :isScreenWide=\"isOver1200px\"\r\n         href=\"tel:+60332897297\"\r\n         :src=\"host.res('icon/telephone-color.svg')\"\r\n         title1=\"Call 03-3289-7297\"\r\n         title2=\"Office\"\r\n      />\r\n      <ItemFloatingContactLink\r\n         class=\"PageHome-floating-phone\"\r\n         :class=\"[isOver900px ? 'PageHome-floating-phone-isScreenWide' : '']\"\r\n         :isScreenWide=\"isOver700px\"\r\n         href=\"tel:+60 16-795-9444\"\r\n         :src=\"host.res('icon/call-color.svg')\"\r\n         title1=\"Call 016-795-9444\"\r\n         title2=\"Mr Beh\"\r\n      />\r\n      <ItemFloatingContactLink\r\n         class=\"PageHome-floating-whatsapp\"\r\n         :isScreenWide=\"isOver700px\"\r\n         :href=\"whatsappLink\"\r\n         target=\"__blank\"\r\n         :src=\"host.res('icon/whatsapp-color.svg')\"\r\n         title1=\"Chat on Whatsapp\"\