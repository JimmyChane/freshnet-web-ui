geHome_Section.vue\";\r\n   import SectionProduct from \"./PageHome_SectionProduct.vue\";\r\n   import SectionProduct2 from \"./PageHome_SectionProduct2.vue\";\r\n   import SectionLocation from \"./PageHome_SectionLocation.vue\";\r\n   import SectionLocation2 from \"./PageHome_SectionLocation2.vue\";\r\n\r\n   import ItemSearchProduct from \"./ItemSearchProduct.vue\";\r\n   import ItemSearchCategory from \"./ItemSearchCategory.vue\";\r\n   import ItemSearchBrand from \"./ItemSearchBrand.vue\";\r\n   import ItemSearchPs2Disc from \"./ItemSearchPs2Disc.vue\";\r\n   import ItemSearchService from \"./ItemSearchService.vue\";\r\n\r\n   import ItemFloatingContactLink from \"./ItemFloatingContactLink.vue\";\r\n\r\n   export default {\r\n      key: \"home\",\r\n      name: \"Home\",\r\n      title: \"Home\",\r\n      icon: { light: \"home-FFFFFF\", dark: \"home-2A4858\" },\r\n\r\n      components: {\r\n         Actionbar,\r\n         PopupWindow,\r\n         SearchInput,\r\n         ItemSearchProduct,\r\n         ItemSearchCategory,\r\n         ItemSearchBrand,\r\n         ItemSearchPs2Disc,\r\n         ItemSearchService,\r\n         Section,\r\n         SectionProduct,\r\n         SectionProduct2,\r\n         SectionLocation,\r\n         SectionLocation2,\r\n         ItemFloatingContactLink,\r\n         Footer,\r\n      },\r\n      data() {\r\n         return { scrollTop: 0, searchText: \"\", searches: [] };\r\n      },\r\n      computed: {\r\n         isScrolledDown: (c) => c.scrollTop > 50,\r\n         whatsappLink: (c) => {\r\n            let phone = \"60167959444\";\r\n            return `https://api.whatsapp.com/send?phone=${phone}`;\r\n         },\r\n\r\n         user: (c) => c.loginStore.getters.user,\r\n\r\n         categories: (c) =>\r\n            c.productStore.getters.groups.filter(\r\n               (group) => group.items.filter((item) => item.isStockAvailable()).length\r\n            ),\r\n         products: (c) =>\r\n            c.productStore.getters.products.filter((product) =>\r\n               product.isStockAvailable()\r\n            ),\r\n         brands: (c) => c.brandStore.getters.items,\r\n         ps2Discs: (c) => c.ps2Store.getters.items,\r\n         services: (c) => {\r\n            if (!c.user) return [];\r\n            return c.serviceStore.getters.items;\r\n         },\r\n\r\n         isOver700px: (c) => c.$root.window.innerWidth > 700,\r\n         isOver900px: (c) => c.$root.window.innerWidth > 900,\r\n         isOver1200px: (c) => c.$root.window.innerWidth > 1200,\r\n      },\r\n      watch: {\r\n         user() {\r\n            if (this.user) {\r\n               if (this.user) this.serviceStore.dispatch(\"getItems\");\r\n            }\r\n\r\n            this.search(this.searchText);\r\n         },\r\n      },\r\n      methods: {\r\n         search(text) {\r\n            text = this.searchText = text.toLowerCase();\r\n            let texts = text.split(/[\\s,]+/).filter((text) => text.trim().length);\r\n\r\n            let productSearches = this.products.map((product) => {\r\n               return {\r\n                  dataType: \"product\",\r\n                  item: product,\r\n                  count: product.search_v2(texts),\r\n               };\r\n            });\r\n            let categorySearches = this.categories.map((category) => {\r\n               return {\r\n                  dataType: \"category\",\r\n                  item: category,\r\n                  count: category.search(texts),\r\n               };\r\n            });\r\n            let brandSearches = this.brands.map((brand) => {\r\n               return {\r\n                  dataType: \"brand\",\r\n                  item: brand,\r\n                  count: brand.search(texts),\r\n               };\r\n            });\r\n            let ps2DiscSearches = this.ps2Discs.map((ps2Disc) => {\r\n               return {\r\n                  dataType: \"ps2Disc\",\r\n                  item: ps2Disc,\r\n                  count: ps2Disc.search(texts),\r\n               };\r\n            });\r\n            let serviceSearches = this.services.map((service) => {\r\n               return {\r\n                  dataType: \"service\",\r\n                  item: service,\r\n                  count: service.search_v2(texts),\r\n               };\r\n            });\r\n\r\n            const searches = [\r\n               ...productSearches,\r\n               ...categorySearches,\r\n               ...brandSearches,\r\n               ...ps2DiscSearches,\r\n               ...serviceSearches,\r\n            ];\r\n            const highCount = searches.reduce((highCount, x) => {\r\n               return highCount < x.count ? x.count : highCount;\r\n            }, 0);\r\n            const valueToAccept = highCount * 0.5;\r\n\r\n            if (valueToAccept)\r\n               this.searches = searches\r\n                  .filter((x) => x.count >= valueToAccept)\r\n                  .sort((x1, x2) => x2.count - x1.count);\r\n            else this.searches = [];\r\n         },\r\n         printElement(element) {\r\n            var mywindow = window.open(\"\", \"PRINT\", \"height=400,width=600\");\r\n\r\n            mywindow.document.write(\"<html><head><title>\" + document.title + \"</title>\");\r\n            mywindow.document.write(\"</head><body >\");\r\n            mywindow.document.write(\"<h1>\" + document.title + \"</h1>\");\r\n            mywindow.document.write(element.innerHTML);\r\n            mywindow.document.write(\"</body></html>\");\r\n\r\n            mywindow.document.close(); // necessary for IE >= 10\r\n            mywindow.focus(); // necessary for IE >= 10*/\r\n\r\n            mywindow.print();\r\n            mywindow.close();\r\n\r\n            return true;\r\n         },\r\n      },\r\n      mounted() {\r\n         this.ps2Store.dispatch(\"getItems\");\r\n         this.brandStore.dispatch(\"getItems\");\r\n         this.categoryStore.dispatch(\"getItems\");\r\n         this.productStore.dispatch(\"getProductsGroupByCategories\");\r\n         if (this.user) {\r\n            if (this.user) this.serviceStore.dispatch(\"getItems\");\r\n         }\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div\r\n      :class=\"[\r\n         'PageHome',\r\n         isOver700px ? 'PageHome-isOver700px' : '',\r\n         isOver900px ? 'PageHome-isOver900px' : '',\r\n         isOver1200px ? 'PageHome-isOver1200px' : '',\r\n      ]\"\r\n   >\r\n      <div\r\n         class=\"Home-scroll\"\r\n         :class=\"[`Home-scroll-isScrolled${isScrolledDown ? 'Down' : 'Up'}`]\"\r\n         @scroll=\"(event) => (scrollTop = event.target.scrollTop)\"\r\n      >\r\n         <div class=\"Home-header-background\">\r\n            <div class=\"Home-header-background-body\">\r\n               <img\r\n                  class=\"Home-header-background-image\"\r\n                  :style=\"{ transform: `translateY(${scrollTop / 3}px)` }\"\r\n                  :src=\"host.res('background/store_front.webp')\"\r\n               />\r\n               <div class=\"Home-header-background-tint\"></div>\r\n            </div>\r\n         </div>\r\n\r\n         <Actionbar\r\n            class=\"Home-actionbar\"\r\n            :leftMenus=\"[\r\n               $root.navigation.isDrawer()\r\n                  ? {\r\n                       title: 'Hamburger Menu',\r\n                       icon: host.res(\r\n                          `icon/hamburgerMenu-${isScrolledDown ? '2A4858' : 'FFFFFF'}.svg`\r\n                       ),\r\n                       click: () => $root.openNavigationDrawer(),\r\n                    }\r\n                  : null,\r\n            ]\"\r\n         >\r\n            <div class=\"Home-actionbar-body\">\r\n               <SearchInput\r\n                  class=\"Home-actionbar-search\"\r\n                  placeholder=\"Search Freshnet Enterprise\"\r\n                  :list=\"searches\"\r\n                  @callback-search=\"(text) => search(text)\"\r\n               >\r\n