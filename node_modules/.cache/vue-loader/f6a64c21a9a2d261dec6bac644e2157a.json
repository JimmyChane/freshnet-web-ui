                 />\r\n                     </div>\r\n                  </div>\r\n               </div>\r\n\r\n               <div class=\"PanelService-section-description\">\r\n                  <div class=\"PanelService-section-description-header\">\r\n                     <span class=\"PanelService-title\">Description</span>\r\n                     <ButtonIcon\r\n                        :src=\"host.res('icon/edit-505050.svg')\"\r\n                        @click=\"actions.onClickUpdateDescription(description)\"\r\n                     />\r\n                  </div>\r\n                  <div\r\n                     class=\"PanelService-section-description-content\"\r\n                     v-if=\"description\"\r\n                  >\r\n                     <p class=\"PanelService-section-description-content-span\">{{\r\n                        description\r\n                     }}</p>\r\n                  </div>\r\n               </div>\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"PanelService-section-belonging\">\r\n            <div class=\"PanelService-section-belonging-header\">\r\n               <span class=\"PanelService-title\">Belongings</span>\r\n               <ButtonIcon\r\n                  :src=\"host.res('icon/edit-505050.svg')\"\r\n                  @click=\"actions.onClickUpdateBelongings(belongings)\"\r\n               />\r\n            </div>\r\n            <div class=\"PanelService-section-belonging-list\" v-if=\"belongings.length\">\r\n               <ItemBelonging\r\n                  v-for=\"belonging in belongings\"\r\n                  :key=\"belonging.title\"\r\n                  :belonging=\"belonging\"\r\n               />\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"PanelService-section-image\">\r\n            <span class=\"PanelService-title\">Image</span>\r\n            <div\r\n               class=\"PanelService-section-image-list\"\r\n               :style=\"{\r\n                  '--image-width': imageFiles.length ? '80px' : '100px',\r\n                  '--image-height': imageFiles.length ? '160px' : '100px',\r\n               }\"\r\n            >\r\n               <ButtonAddImage\r\n                  class=\"PanelService-section-image-add\"\r\n                  @callback-result=\"onImageAdd\"\r\n               />\r\n               <ButtonImage\r\n                  class=\"PanelService-section-image-list-item-button\"\r\n                  v-for=\"imageFile in imageFiles\"\r\n                  :key=\"imageFile.name\"\r\n                  :src=\"host.imgFile.name(imageFile.name, { height: 160 })\"\r\n                  @click=\"actions.onClickOpenImage(imageFile)\"\r\n                  @click-remove=\"actions.onClickRemoveImage(imageFile)\"\r\n               />\r\n            </div>\r\n         </div>\r\n\r\n         <PanelEvents\r\n            class=\"PanelService-PanelEvents\"\r\n            v-if=\"service\"\r\n            :service=\"service\"\r\n            @click-add-event=\"actions.onClickAddEvent()\"\r\n            @click-remove-event=\"\r\n               (event) => actions.onClickRemoveEvent({ service, event })\r\n            \"\r\n         />\r\n      </div>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   // abstract\r\n   .PanelService-title {\r\n      font-weight: 900;\r\n      font-size: 1rem;\r\n      color: black;\r\n   }\r\n\r\n   .PanelService {\r\n      width: unset;\r\n      height: 100%;\r\n      min-width: 100%;\r\n      display: flex;\r\n      flex-direction: column;\r\n      line-height: 1.2;\r\n      position: relative;\r\n      background: hsl(0, 0%, 90%);\r\n      overflow-y: auto;\r\n\r\n      .PanelService-actionbar {\r\n         --actionbar-background-color-translucent: white;\r\n         border-bottom: 1px solid hsl(0, 0%, 80%);\r\n         z-index: 3;\r\n         .PanelService-actionbar-state {\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: center;\r\n            .PanelService-actionbar-state-selector {\r\n               width: 100%;\r\n               max-width: 12rem;\r\n               flex-grow: 1;\r\n            }\r\n         }\r\n      }\r\n\r\n      .PanelService-tint {\r\n         opacity: 0.07;\r\n      }\r\n\r\n      .PanelService-body {\r\n         z-index: 2;\r\n         padding-top: 1.2em;\r\n         display: flex;\r\n         flex-direction: column;\r\n         gap: 20px;\r\n         --section-padding: 1.2em;\r\n         --section-horizontal-padding: 0 1.2em;\r\n\r\n         .PanelService-section-header {\r\n            width: 100%;\r\n            display: flex;\r\n            flex-direction: row;\r\n            flex-wrap: nowrap;\r\n            flex-grow: 1;\r\n            align-items: flex-start;\r\n            justify-content: space-between;\r\n            gap: 1.2em;\r\n            padding: var(--section-horizontal-padding);\r\n\r\n            .PanelService-section-label {\r\n               width: max-content;\r\n               display: flex;\r\n               flex-direction: row;\r\n               align-items: flex-start;\r\n               justify-content: flex-start;\r\n               gap: 0.1em;\r\n\r\n               .PanelService-section-labels {\r\n                  min-height: 2.5em;\r\n                  display: flex;\r\n                  flex-direction: row;\r\n                  align-items: center;\r\n                  justify-content: flex-start;\r\n                  gap: 0.1em;\r\n                  & > * {\r\n                     display: flex;\r\n                     flex-direction: row;\r\n                     align-items: center;\r\n                     justify-content: center;\r\n                     gap: 0.3em;\r\n                     background: var(--primary-color);\r\n                     color: white;\r\n                     font-size: 0.8em;\r\n                     padding: 0.2em 0.4em;\r\n                     border-radius: 0.2em;\r\n                     cursor: pointer;\r\