temBelonging.vue\";\r\n\r\n   import PanelEvents from \"./PanelEvents.vue\";\r\n\r\n   import ServiceState from \"@/items/ServiceState.js\";\r\n   import PhoneNumber from \"@/items/PhoneNumber.js\";\r\n\r\n   export default {\r\n      components: {\r\n         Actionbar,\r\n         TintBackground,\r\n         ButtonIcon,\r\n         LinkIcon,\r\n         Selector,\r\n         ButtonOption,\r\n         ButtonAddImage,\r\n         ButtonImage,\r\n         PanelEvents,\r\n         ItemBelonging,\r\n         ButtonIcon,\r\n      },\r\n      props: {\r\n         service: { type: Object, default: () => null },\r\n         actions: { type: Object, default: () => null },\r\n      },\r\n      data() {\r\n         return {\r\n            ServiceState,\r\n            nameOfUser: \"\",\r\n            bookmarkHeaderIconIsHover: false,\r\n         };\r\n      },\r\n      computed: {\r\n         isUrgent: (c) => c.service.isUrgent(),\r\n         isWarranty: (c) => c.service.isWarranty(),\r\n\r\n         customer: (c) => c.service.getCustomer(),\r\n         customerName: (c) => c.customer.name,\r\n         customerPhoneNumber: (c) => new PhoneNumber(c.customer.phoneNumber).toString(),\r\n         isPhoneNumber: (c) => !!c.customerPhoneNumber,\r\n\r\n         description: (c) => c.service.getDescription(),\r\n         belongings: (c) => c.service.getBelongings(),\r\n         imageFiles: (c) => c.service.getImageFiles(),\r\n\r\n         labels: (c) => c.service.getLabels(),\r\n\r\n         stateColor() {\r\n            if (!this.service) return \"inherit\";\r\n            const { state } = this.service;\r\n            const res = ServiceState.list.find((s) => s.key === state);\r\n            if (!res) return \"inherit\";\r\n            return res.color;\r\n         },\r\n      },\r\n      watch: {\r\n         service() {\r\n            this.invalidate();\r\n         },\r\n      },\r\n      methods: {\r\n         async invalidate() {\r\n            this.nameOfUser = await this.getOwnerNameFromItem();\r\n         },\r\n         async getOwnerNameFromItem() {\r\n            const { service } = this;\r\n\r\n            if (!service) return \"\";\r\n\r\n            const name = await service.fetchName().catch((error) => {\r\n               this.$root.feedback(\"Error getting user for service\");\r\n               return \"\";\r\n            });\r\n\r\n            if (service !== this.service) return;\r\n            if (name.length) return name;\r\n\r\n            return \"unknown\";\r\n         },\r\n         onImageAdd(imageFile) {\r\n            this.serviceStore\r\n               .dispatch(\"addImageToId\", {\r\n                  serviceID: this.service.id,\r\n                  imageFile,\r\n               })\r\n               .then((serivce) => {})\r\n               .catch((error) => {\r\n                  this.$root.feedback(\"Failed to Add Image\");\r\n               });\r\n         },\r\n      },\r\n      mounted() {\r\n         this.invalidate();\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div class=\"PanelService\" :style=\"{ '--primary-color': stateColor }\">\r\n      <Actionbar\r\n         v-if=\"service\"\r\n         class=\"PanelService-actionbar\"\r\n         :rightMenus=\"[\r\n            {\r\n               icon: host.res('icon/trash-2A4858.svg'),\r\n               click: () => actions.onClickRemove(service),\r\n            },\r\n            {\r\n               icon: host.res('icon/close-2A4858.svg'),\r\n               click: () => actions.onClickClose(),\r\n            },\r\n         ]\"\r\n      >\r\n         <div class=\"PanelService-actionbar-state\" v-if=\"service\">\r\n            <Selector\r\n               class=\"PanelService-actionbar-state-selector\"\r\n               :list=\"ServiceState.list\"\r\n               :keySelected=\"service.state\"\r\n               @callback-select=\"\r\n                  (state) => {\r\n                     serviceStore.dispatch('updateStateOfId', {\r\n                        serviceID: service.id,\r\n                        state,\r\n                     });\r\n                  }\r\n               \"\r\n            />\r\n         </div>\r\n      </Actionbar>\r\n\r\n      <TintBackground class=\"PanelService-tint\" />\r\n\r\n      <div v-if=\"service\" class=\"PanelService-body\">\r\n         <div class=\"PanelService-section-header\">\r\n            <div class=\"PanelService-section-label\">\r\n               <div class=\"PanelService-section-labels\">\r\n                  <button\r\n                     class=\"PanelService-section-labels-item\"\r\n                     v-for=\"label in labels\"\r\n                     :key=\"label.getTitle()\"\r\n                     :style=\"{ '--primary-color': `#${label.getHexColor()}` }\"\r\n                     @click=\"\r\n                        () => {\r\n                           serviceStore.dispatch('removeLabelFromId', {\r\n                              serviceID: service.getId(),\r\n                              label,\r\n                           });\r\n                        }\r\n                     \"\r\n                  >\r\n                     <span class=\"PanelService-section-labels-item-title\">\r\n                        {{ label.getTitle() }}\r\n                     </span>\r\n                     <img\r\n                        class=\"PanelService-section-labels-item-close\"\r\n                        :src=\"host.res('icon/close-FFFFFF.svg')\"\r\n                     />\r\n                  </button>\r\n               </div>\r\n\r\n               <ButtonOption\r\n                  class=\"PanelService-section-label-option\"\r\n                  direction=\"bottomRight\"\r\n                  @mouseover=\"bookmarkHeaderIconIsHover = true\"\r\n                  @mouseleave=\"bookmarkHeaderIconIsHover = false\"\r\n                  :menus=\"\r\n                     [\r\n                        {\r\n                           key: 'urgent',\r\n                           title: 'Urgent',\r\n                           click() {\r\n                              serviceStore.dispatch('updateUrgentOfId', {\r\n                                 serviceID: service.getId(),\r\n                                 isUrgent: !service.isUrgent(),\r\n                              });\r\n                           },\r\n                        },\r\n                        {\r\n                           key: 'warranty',\r\n                           title: 'Warranty',\r\n                           click() {\r\n                              serviceStore.dispatch('updateWarrantyOfId', {\r\n                                 serviceID: service.getId(),\r\n                                 isWarranty: !service.isWarranty(),\r\n                              });\r\n                           },\r\n                        },\r\n                     ].filter((menu) => {\r\n                        const label = labels.find(\r\n                           (label) => label.getTitle() === menu.title\r\n                        );\r\n                        return !label;\r\n                     })\r\n                  \"\r\n               >\r\n                  <img\r\n                     class=\"PanelService-section-label-option-icon\"\r\n                     :src=\"\r\n                        bookmarkHeaderIconIsHover\r\n                           ? host.res('icon/bookmark-add-505050.svg')\r\n                           : host.res('icon/bookmark-505050.svg')\r\n                     \"\r\n                  />\r\n               </ButtonOption>\r\n            </div>\r\n\r\n            <div class=\"PanelService-section-info\" v-if=\"nameOfUser\">\r\n               <div class=\"PanelService-section-timestamp\" v-if=\"service\">\r\n                  {{ service.getTimestamp() }}\r\n               </div>\r\n               <span class=\"PanelService-section-user\">{{ nameOfUser }}</span>\r\n            </div>\r\n         </div>\r\n\r\n         <div class=\"PanelService-section-