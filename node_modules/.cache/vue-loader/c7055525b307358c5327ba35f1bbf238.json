urn { nameOfUser: \"unknown\" };\r\n      },\r\n      computed: {\r\n         timestamp: (c) => c.item.getTimestamp(),\r\n         description: (c) => c.item.getDescription(),\r\n         methodPrimaryColor: (c) => {\r\n            if (c.item.isInfo()) return \"#0771d2\";\r\n            if (c.item.isQuotation()) return \"#961d96\";\r\n            if (c.item.isPurchase()) return \"#258915\";\r\n            return \"\";\r\n         },\r\n         methodTitle: (c) => {\r\n            if (c.item.isInfo()) return \"Info\";\r\n            if (c.item.isQuotation()) return \"Quotation\";\r\n            if (c.item.isPurchase()) return \"Purchase\";\r\n            return \"\";\r\n         },\r\n         methodResult: (c) => {\r\n            const result = c.item.getResult();\r\n            return result ? result : \"\";\r\n         },\r\n      },\r\n      methods: {\r\n         onUser() {\r\n            if (!this.item) {\r\n               this.nameOfUser = \"\";\r\n               return;\r\n            }\r\n\r\n            let nameOfUser = this.item.getNameOfUser();\r\n            let username = this.item.getUsername();\r\n\r\n            if (nameOfUser) {\r\n               this.nameOfUser = nameOfUser;\r\n            } else {\r\n               this.userStore\r\n                  .dispatch(\"getUserByUsername\", { username })\r\n                  .then((user) => {\r\n                     this.nameOfUser = user ? user.name : \"Unknown\";\r\n                  })\r\n                  .catch((error) => {\r\n                     this.$root.feedback(\"An Unknown User\");\r\n                     this.nameOfUser = \"Unknown\";\r\n                  });\r\n            }\r\n         },\r\n      },\r\n      mounted() {\r\n         this.onUser();\r\n      },\r\n   };\r\n</script>\r\n\r\n<template>\r\n   <div class=\"ItemEvent\" :style=\"{ '--primary-color': methodPrimaryColor }\">\r\n      <div class=\"ItemEvent-top\" v-if=\"methodResult || nameOfUser\">\r\n         <div class=\"ItemEvent-results\" v-if=\"methodResult\">\r\n            <span v-if=\"methodTitle\">{{ `${methodTitle}: ${methodResult}` }}</span>\r\n            <span v-else>{{ `${methodResult}` }}</span>\r\n         </div>\r\n\r\n         <!-- <span class=\"ItemEvent-user\" v-if=\"nameOfUser\">{{ nameOfUser }}</span> -->\r\n\r\n         <div class=\"ItemEvent-top-dot-body\">\r\n            <div class=\"ItemEvent-state-dot\"></div>\r\n         </div>\r\n\r\n         <MenuButton\r\n            :menus=\"[\r\n               {\r\n                  key: 'delete',\r\n                  title: 'Delete',\r\n                  icon: host.res('icon/page/service/rejected-color.svg'),\r\n                  interact: () => $emit('callback-delete', item),\r\n               },\r\n            ]\"\r\n         />\r\n      </div>\r\n\r\n      <div class=\"ItemEvent-bottom\" v-if=\"description && timestamp\">\r\n         <span class=\"ItemEvent-description\" v-if=\"description\">{{ description }}</span>\r\n         <span class=\"ItemService-timestamp\">{{ timestamp }}</span>\r\n      </div>\r\n   </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n   .ItemEvent {\r\n      display: flex;\r\n      flex-direction: column;\r\n      flex-wrap: nowrap;\r\n      border-radius: 6px;\r\n      background: white;\r\n      border: 0.1rem solid hsla(0, 0%, 0%, 0.2);\r\n\r\n      .ItemEvent-top {\r\n         // display: flex;\r\n         // flex-direction: row;\r\n         // align-items: flex-start;\r\n         // justify-content: flex-end;\r\n         // flex-wrap: nowrap;\r\n         // gap: 2em;\r\n         // padding: 0.8rem;\r\n         // border-bottom: 0.05em solid hsla(0, 0%, 0%, 0.1);\r\n\r\n         padding: 0.2em;\r\n         padding-left: 0.6em;\r\n         border-bottom: 0.05em solid hsla(0, 0%, 0%, 0.1);\r\n         display: flex;\r\n         flex-direction: row;\r\n         justify-content: flex-end;\r\n         align-items: center;\r\n\r\n         .ItemEvent-results {\r\n            width: 100%;\r\n            flex-grow: 1;\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: flex-start;\r\n            justify-content: flex-start;\r\n            gap: 0.2em;\r\n\r\n            & > * {\r\n               background: var(--primary-color);\r\n               color: white;\r\n               text-align: start;\r\n               font-size: 0.8em;\r\n               padding: 0.2em 0.5em;\r\n               border-radius: 0.3em;\r\n            }\r\n         }\r\n\r\n         .ItemEvent-user {\r\n            min-width: max-content;\r\n            flex-grow: 0;\r\n            display: flex;\r\n            flex-direction: row;\r\n            align-items: center;\r\n            justify-content: flex-end;\r\n            gap: 0.8rem;\r\n            color: #6f6f6f;\r\n            font-size: 0.8rem;\r\n            text-align: end;\r\n         }\r\n\r\n         .ItemEvent-top-dot-body {\r\n            display: flex;\r\n            flex-direction: column;\r\n            align-items: center;\r\n            justify-content: flex-end;\r\n            padding: 0 1em;\r\n            .ItemEvent-state-dot {\r\n               --size: 0.5em;\r\n               min-width: var(--size);\r\n               min-height: var(--size);\r\n               width: var(--size);\r\n               height: var(--size);\r\n               background: var(--primary-color);\r\n               border-radius: var(--size);\r\n            }\r\n         }\r\n      }\r\n      .ItemEvent-bottom {\r\n         padding: 1em;\r\n         padding: 0.6em;\r\n         display: flex;\r\n         flex-direction: row;\r\n         align-items: flex-start;\r\n         justify-content: flex-end;\r\n         gap: 0.5em;\r\n         .ItemEvent-description {\r\n            width: 100%;\r\n            flex-grow: 1;\r\n            display: flex;\r\n            flex-direction: column;\r\n            line-height: 1.1;\r\n            font-size: 0.9rem;\r\n            white-space: pre-line;\r\n         }\r\n         .ItemService-timestamp {\r\n            flex-grow: 0;\r\n            min-width: max-content;\r\n            width: max-content;\r\n            font-size: 0.7em;\r\n            color: rgb(112, 112, 112);\r\n         }\r\n      }\r\n   }\r\n</style>\r\n"]}]}