ustify-content: center;\r\n\r\n               .Actionbar-tab-title {\r\n                  z-index: 1;\r\n                  transition: var(--animation-duration);\r\n                  text-align: center;\r\n                  text-overflow: ellipsis;\r\n                  white-space: nowrap;\r\n                  overflow: hidden;\r\n                  font-weight: 900;\r\n                  color: var(--primary-color);\r\n               }\r\n               .Actionbar-tab-icon {\r\n                  z-index: 2;\r\n                  width: 1.5rem;\r\n                  height: 1.5rem;\r\n                  padding: 0.25rem;\r\n               }\r\n            }\r\n            .Actionbar-tab-isSelected {\r\n               width: 8.2rem;\r\n               gap: 0.5rem;\r\n               padding-top: 0.1rem;\r\n               background-color: #f6f6f6;\r\n\r\n               .Actionbar-tab-title {\r\n                  width: 4.8rem;\r\n               }\r\n            }\r\n            .Actionbar-tab-isDeselected {\r\n               width: 2.8rem;\r\n               gap: 0;\r\n               padding-top: 0.1rem;\r\n               background-color: transparent;\r\n               cursor: pointer;\r\n\r\n               .Actionbar-tab-title {\r\n                  width: 0;\r\n                  height: 0;\r\n                  opacity: 0;\r\n               }\r\n\r\n               &:hover {\r\n                  background-color: hsla(0, 0%, 96%, 0.5);\r\n               }\r\n            }\r\n         }\r\n      }\r\n\r\n      .Actionbar-toolbar {\r\n         width: 100%;\r\n         padding: 0.3rem 1rem;\r\n         background-color: #f6f6f6;\r\n         gap: 1rem;\r\n\r\n         z-index: 1;\r\n\r\n         display: flex;\r\n         flex-direction: column;\r\n         align-items: center;\r\n         justify-content: center;\r\n\r\n         & > * {\r\n            width: 100%;\r\n            max-width: var(--max-width);\r\n            display: flex;\r\n            flex-direction: row;\r\n 